<stdESA/9b2d1e12-31f7-4b4b-b86d-8789bc8dc003> a skos:Concept ;
    skos:broader <stdESA/37abc2fb-d453-49ca-9e4a-e315dca3362d> ;
    skos:inScheme <> ;
    skos:notation "2.1.1" ;
