@base <https://w3id.org/iqb/bista22des1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<ab/3f8de107-6349-4029-989c-c10fd07d89e7> a skos:Concept ;
    skos:broader <ab/> ;
    skos:definition "Umfasst das selbstständige Anordnen, Verarbeiten, Erklären und Darstellen bekannter Sachverhalte (auch auf der Basis von Texten unterschiedlicher medialer Form) unter vorgegebenen Gesichtspunkten in einem durch Übung bekannten Zusammenhang und das selbstständige Übertragen und Anwenden des Gelernten auf vergleichbare neue Zusammenhänge und Sachverhalte."@de ;
    skos:inScheme <> ;
    skos:notation "2" ;
    skos:prefLabel "Anforderungsbereich II"@de .

<ab/8768283e-6bf0-44e4-b8b8-00a01e224897> a skos:Concept ;
    skos:broader <ab/> ;
    skos:definition "Umfasst das Wiedergeben von Sachverhalten, Textinhalten und Kenntnissen im gelernten Zusammenhang, die Verständnissicherung sowie das Anwenden und Beschreiben geübter Arbeitstechniken und Verfahren."@de ;
    skos:inScheme <> ;
    skos:notation "1" ;
    skos:prefLabel "Anforderungsbereich I"@de .

<ab/f9e1c5e1-26d0-425a-bc49-c5411866d69c> a skos:Concept ;
    skos:broader <ab/> ;
    skos:definition "Umfasst das Verarbeiten komplexer Sachverhalte (auch auf der Basis von Texten unterschiedlicher medialer Form) mit dem Ziel, zu selbstständigen Lösungen, Gestaltungen oder Deutungen, Folgerungen, Verallgemeinerungen, Begründungen und Wertungen zu gelangen. Dabei wählen die Schülerinnen und Schüler selbstständig geeignete Arbeitstechniken, Strategien und Verfahren zur Bewältigung der Aufgabe, wenden sie auf eine neue Problemstellung an und reflektieren das eigene Vorgehen."@de ;
    skos:inScheme <> ;
    skos:notation "3" ;
    skos:prefLabel "Anforderungsbereich III"@de .

<stdESA/04017ad8-7eb4-4430-8405-1db940620d85> a skos:Concept ;
    skos:broader <stdESA/4658e686-abb9-488b-b925-aaf7f7791ca8> ;
    skos:inScheme <> ;
    skos:notation "4.5.5" ;
    skos:prefLabel "stellen an ausgewählten Beispielen Zusammenhänge zwischen Intentionen, Textmerkmalen, sprachlichen Gestaltungsmitteln und Wirkungen her,"@de .

<stdESA/047ffc51-b4ad-47b7-b770-5e0b7910a5fb> a skos:Concept ;
    skos:broader <stdESA/446c5c43-8c26-455c-9001-a8bb2e2066c6> ;
    skos:inScheme <> ;
    skos:notation "2.5.8" ;
    skos:prefLabel "formulieren eigene Positionen und begründen diese"@de .

<stdESA/05083170-7950-4657-ad57-290a64fbcf58> a skos:Concept ;
    skos:broader <stdESA/7201b260-b8ae-4681-abd3-ce9803b22f80> ;
    skos:inScheme <> ;
    skos:notation "4.6.4" ;
    skos:prefLabel "untersuchen am Beispiel den Zusammenhang von digitalen Nutzerprofilen, Algorithmen, Gestaltung und Wirkung digitaler Angebote und bewerten ihn,"@de .

<stdESA/064b0a77-0ff3-409b-bc0c-a4898cfd682c> a skos:Concept ;
    skos:broader <stdESA/05945a07-990f-4430-bf2a-85a6c264ad91> ;
    skos:inScheme <> ;
    skos:notation "4.4.4" ;
    skos:prefLabel "formulieren Wertungen von Texten und begründen sie,"@de .

<stdESA/09fbb14d-5d9e-4318-a86f-c7a831662456> a skos:Concept ;
    skos:broader <stdESA/eaad13ff-350d-49fc-a955-139420ddd0ba> ;
    skos:inScheme <> ;
    skos:notation "1.3.2" ;
    skos:prefLabel "beachten gemeinsam entwickelte oder für öffentliche Kommunikation prototypische Gesprächsregeln; gestalten eine wertschätzende Gesprächsatmosphä¬re,"@de .

<stdESA/0c0aae17-141c-4e5e-85d3-30f4945a389b> a skos:Concept ;
    skos:broader <stdESA/effcdc0f-1770-40f0-a9a6-1656a12707d3> ;
    skos:inScheme <> ;
    skos:notation "4.1.1" ;
    skos:prefLabel "kennen Werke bedeutender Autorinnen und Autoren und berichten über ausgewählte Texte und ihre Leseerfahrungen,"@de .

<stdESA/0ed695a8-f079-4483-9876-a838f30304d4> a skos:Concept ;
    skos:broader <stdESA/f0eb4a10-1c8f-4ea5-808c-f8565646fb61> ;
    skos:inScheme <> ;
    skos:notation "3.1.4" ;
    skos:prefLabel "lesen in angemessenem Tempo."@de .

<stdESA/0fd663f8-f42e-4c56-b624-40dafc275ad0> a skos:Concept ;
    skos:broader <stdESA/a203e7dd-1858-4178-a9d4-cf3b6eae3b24> ;
    skos:inScheme <> ;
    skos:notation "1.2.1" ;
    skos:prefLabel "können in vertrauten und unvertrauten Kommunikationssituationen"@de .

<stdESA/119d3bce-ef5a-4418-98ec-08ad37ed16db> a skos:Concept ;
    skos:broader <stdESA/350af187-334d-41f1-bcc9-ea50ca8c316d> ;
    skos:inScheme <> ;
    skos:notation "1.1.9" ;
    skos:prefLabel "nutzen technische Möglichkeiten beim digitalen Kommunizieren in vertrauten und unvertrauten Situationen."@de .

<stdESA/14248b85-1ba3-4974-b933-17e9b682a925> a skos:Concept ;
    skos:broader <stdESA/2b8af6a6-145c-458e-80b8-d20fc4cc0403> ;
    skos:inScheme <> ;
    skos:notation "4.2.1" ;
    skos:prefLabel "nutzen unterschiedliche Informationsangebote und Recherchemöglichkeiten (z. B. Kritiken, Bestenlisten und andere Empfehlungen; Internet, Zeitschriften, Zeitungen, Hörfunk, Fernsehen, Bücherei),"@de .

<stdESA/1711bebf-01af-4435-bac8-cbc420d7bb84> a skos:Concept ;
    skos:broader <stdESA/446c5c43-8c26-455c-9001-a8bb2e2066c6> ;
    skos:inScheme <> ;
    skos:notation "2.5.2" ;
    skos:prefLabel "formulieren Texte auch für unbekannte und unterschiedliche Adressaten angemessen, "@de .

<stdESA/19ece7ef-b2db-49c7-983e-302ab5e3c7dc> a skos:Concept ;
    skos:broader <stdESA/05945a07-990f-4430-bf2a-85a6c264ad91> ;
    skos:inScheme <> ;
    skos:notation "4.4.11" ;
    skos:prefLabel "gestalten Texte unterschiedlicher medialer Form szenisch,"@de .

<stdESA/1a5ad8f7-8cb1-41b8-90e1-8c39f5b84198> a skos:Concept ;
    skos:broader <stdESA/4658e686-abb9-488b-b925-aaf7f7791ca8> ;
    skos:inScheme <> ;
    skos:notation "4.5.7" ;
    skos:prefLabel "ermitteln an ausgewählten Beispielen die Wirkungsabsichten von Texten und beurteilen diese."@de .

<stdESA/1cd15c87-4a85-4ded-b211-1b51ef68a691> a skos:Concept ;
    skos:broader <stdESA/de5ef34d-4139-4021-9c57-f872668c4334> ;
    skos:inScheme <> ;
    skos:notation "2.2.4" ;
    skos:prefLabel "schreiben Satzanfänge, Eigennamen und prototypische Kerne von Nominalgruppen groß,"@de .

<stdESA/1e698c31-96d2-4b12-88d5-1d94d3767bf1> a skos:Concept ;
    skos:broader <stdESA/26002dd8-6ee4-4892-89d7-8c171f41c223> ;
    skos:inScheme <> ;
    skos:notation "2.4.3" ;
    skos:prefLabel "stellen Überlegungen zu den Adressaten (unbekannt, verschiedene), deren unterschiedlichem Vorwissen und ihren Lesezielen an,"@de .

<stdESA/26ea3581-c908-4892-9769-e2acc2d28367> a skos:Concept ;
    skos:broader <stdESA/350af187-334d-41f1-bcc9-ea50ca8c316d> ;
    skos:inScheme <> ;
    skos:notation "1.1.8" ;
    skos:prefLabel "geben in vertrauten und unvertrauten Situationen nach vorgegebenen Kriterien gezielt Feedback zu Redebeiträgen,"@de .

<stdESA/2dbd0883-bc62-4e24-b7b4-6cd33b4839f3> a skos:Concept ;
    skos:broader <stdESA/effcdc0f-1770-40f0-a9a6-1656a12707d3> ;
    skos:inScheme <> ;
    skos:notation "4.1.4" ;
    skos:prefLabel "stellen an einem Beispiel Zusammenhänge zwischen Text und Kontext her (insbes. Entstehungszeit, Leben des Autors/der Autorin),"@de .

<stdESA/303ce5dd-68fe-4064-afa3-669b71f27796> a skos:Concept ;
    skos:broader <stdESA/ffddb20e-437c-4024-86d8-ff6426e8e0a6> ;
    skos:inScheme <> ;
    skos:notation "3.4.3" ;
    skos:prefLabel "bereiten das Lesen nach (u. a. zentrale Textaussagen wiedergeben, wertende Einschätzung zum Text abgeben, anfängliche Erwartungen und Annahmen zur Textaussage prüfen und ggf. korrigieren, eigenen Wissenszuwachs prüfen)."@de .

<stdESA/30a50661-eeca-426c-a378-247736fbf883> a skos:Concept ;
    skos:broader <stdESA/effcdc0f-1770-40f0-a9a6-1656a12707d3> ;
    skos:inScheme <> ;
    skos:notation "4.1.2" ;
    skos:prefLabel "unterscheiden epische, lyrische und dramatische Texte, insbesondere epische Kleinformen (z. B. Fabel, Kurzgeschichte), Erzählung, (Jugend-)Roman, Gedichte, Drama,"@de .

<stdESA/33cd9055-de16-430e-9ccc-d94a8e3e5a1b> a skos:Concept ;
    skos:broader <stdESA/26002dd8-6ee4-4892-89d7-8c171f41c223> ;
    skos:inScheme <> ;
    skos:notation "2.4.4" ;
    skos:prefLabel "klären entsprechend dem Schreibziel die Schreibabsicht, Schreibsituation und Textfunktion,"@de .

<stdESA/36b00119-0802-424e-adc6-ecb39886cdb0> a skos:Concept ;
    skos:broader <stdESA/446c5c43-8c26-455c-9001-a8bb2e2066c6> ;
    skos:inScheme <> ;
    skos:notation "2.5.7" ;
    skos:prefLabel "nehmen Gedanken und deutlich markierte Positionen anderer in den eigenen Text auf und positionieren sich zu ihnen,"@de .

<stdESA/37d8934e-97a2-4fdd-9851-8d2ecfa49c73> a skos:Concept ;
    skos:broader <stdESA/2579ea69-b20a-4bd7-b4ff-3c75748ed2bf> ;
    skos:definition "Die Schülerinnen und Schüler beachten bei der Planung und Strukturierung, beim Formulieren und beim Überarbeiten von Texten allgemeine Textmerkmale, Adressaten, den Schreibanlass, Schreibfunktion und Textsorte, Formen des Schreibens und den Einsatz digitaler Schreibwerkzeuge, den Umgang mit Quellen beim Belegen und Zitieren und die Anwendung von Schreibstrategien."@de ;
    skos:inScheme <> ;
    skos:notation "2.3" ;
    skos:prefLabel "Texte verfassen"@de .

<stdESA/397b0af2-2698-49e2-85dd-75daab5fe930> a skos:Concept ;
    skos:broader <stdESA/27acf01e-82d3-4302-a65e-cd1db51f5f01> ;
    skos:inScheme <> ;
    skos:notation "1.4.4" ;
    skos:prefLabel "planen und gestalten Formen des szenischen Sprechens und Spielens, z. B. szenische Lesung, Szene, Aufführung, Hörspiel, Videoclip, Standbild, Improvisation."@de .

<stdESA/3a9879b3-808c-4e96-aec2-109944550f3b> a skos:Concept ;
    skos:broader <stdESA/4658e686-abb9-488b-b925-aaf7f7791ca8> ;
    skos:inScheme <> ;
    skos:notation "4.5.1" ;
    skos:prefLabel "erschließen aus einem oder mehreren klar strukturierten Texten zielgerichtet relevante Informationen, ordnen, selegieren und vergleichen diese und prüfen ihren Sachgehalt,"@de .

<stdESA/3de0509c-6834-4b8c-ad7d-40d8072a65e2> a skos:Concept ;
    skos:broader <stdESA/4658e686-abb9-488b-b925-aaf7f7791ca8> ;
    skos:inScheme <> ;
    skos:notation "4.5.6" ;
    skos:prefLabel "beziehen an ausgewählten Beispielen Textaussagen auf eigene Welt- und Wertvorstellungen und überprüfen diese,"@de .

<stdESA/41ce067a-7964-44cc-959a-a97336366adf> a skos:Concept ;
    skos:broader <stdESA/effcdc0f-1770-40f0-a9a6-1656a12707d3> ;
    skos:inScheme <> ;
    skos:notation "4.1.3" ;
    skos:prefLabel "kennen Merkmale medialer und szenischer Gestaltung (u. a. Musik, Geräusche, Farben, Perspektive, Animation, Bühnenbild) und beschreiben an ausgewählten Beispielen ihre Wirkung,"@de .

<stdESA/44399c6b-85b1-44be-ba49-3d393c1a893e> a skos:Concept ;
    skos:broader <stdESA/7201b260-b8ae-4681-abd3-ce9803b22f80> ;
    skos:inScheme <> ;
    skos:notation "4.6.2" ;
    skos:prefLabel "unterscheiden digitale Angebote in Bezug auf ihre Funktion und Wirkung (u. a. informierend, unterhaltend, appellierend),"@de .

<stdESA/446f4072-245f-454e-bc09-3e8c94b076cb> a skos:Concept ;
    skos:broader <stdESA/eaad13ff-350d-49fc-a955-139420ddd0ba> ;
    skos:inScheme <> ;
    skos:notation "1.3.6" ;
    skos:prefLabel "wenden allgemeine und für Gesprächs- und Lernformen spezifische Strategien an, z. B. Gespräche steuern (Diskussionen), zielführende Rückmeldungen geben (Feedback),"@de .

<stdESA/45163785-dd5a-4e38-9158-c6169fb705fe> a skos:Concept ;
    skos:broader <stdESA/58ff39b2-4511-46ad-a611-dd9bb0ae0da9> ;
    skos:inScheme <> ;
    skos:notation "5.2.3" ;
    skos:prefLabel "Rechtschreibung"@de .

<stdESA/464fc8d1-4032-4aeb-877b-4fecc0a325c4> a skos:Concept ;
    skos:broader <stdESA/de5ef34d-4139-4021-9c57-f872668c4334> ;
    skos:inScheme <> ;
    skos:notation "2.2.3" ;
    skos:prefLabel "schreiben häufige Wörter, auch häufige Fachbegriffe und Fremdwörter, richtig,"@de .

<stdESA/4744372e-3e92-47ec-a7b8-cffb8eb8b38b> a skos:Concept ;
    skos:broader <stdESA/de5ef34d-4139-4021-9c57-f872668c4334> ;
    skos:inScheme <> ;
    skos:notation "2.2.1" ;
    skos:prefLabel "kennen wort- und satzbezogene Rechtschreibstrategien und wenden diese an"@de .

<stdESA/522d1acd-0af9-4288-81c5-1f2c904db5b6> a skos:Concept ;
    skos:broader <stdESA/effcdc0f-1770-40f0-a9a6-1656a12707d3> ;
    skos:inScheme <> ;
    skos:notation "4.1.5" ;
    skos:prefLabel "kennen häufig verwendete sprachliche Gestaltungsmittel (u. a. Wortwahl, Wiederholung, sprachliche Bilder, Vergleich) und beschreiben an ausgewählten Beispielen ihre Wirkung,"@de .

<stdESA/523a2455-26d0-4191-b833-2d9e472d5834> a skos:Concept ;
    skos:broader <stdESA/37abc2fb-d453-49ca-9e4a-e315dca3362d> ;
    skos:inScheme <> ;
    skos:notation "2.1,1" ;
    skos:prefLabel "schreiben Texte (handschriftlich leserlich und mithilfe digitaler Schreibwerkzeuge) in einem der Situation angemessenen Tempo, z. B. persönliche Notiz vs. handschriftlicher Brief,"@de .

<stdESA/53a27d2a-d589-45ce-85dc-752c215a1155> a skos:Concept ;
    skos:broader <stdESA/350af187-334d-41f1-bcc9-ea50ca8c316d> ;
    skos:inScheme <> ;
    skos:notation "1.1.5" ;
    skos:prefLabel "bauen Redebeiträge in vertrauten und unvertrauten Situationen für andere nachvollziehbar und sinnvoll auf,"@de .

<stdESA/562774a4-eff4-4df0-91b9-be0236817dfe> a skos:Concept ;
    skos:broader <stdESA/4658e686-abb9-488b-b925-aaf7f7791ca8> ;
    skos:inScheme <> ;
    skos:notation "4.5.4" ;
    skos:prefLabel "unterscheiden informierende, erzählende und appellierende Elemente in Texten,"@de .

<stdESA/56352328-566f-4050-b7ee-71fac639b159> a skos:Concept ;
    skos:broader <stdESA/7201b260-b8ae-4681-abd3-ce9803b22f80> ;
    skos:inScheme <> ;
    skos:notation "4.6.5" ;
    skos:prefLabel "nutzen digitale Werkzeuge zielgerichtet und reflektiert für die Gestaltung eigener medialer Produkte, z. B. für Präsentationen."@de .

<stdESA/568174c7-8109-4a40-86c1-c6615aa53d1e> a skos:Concept ;
    skos:broader <stdESA/effcdc0f-1770-40f0-a9a6-1656a12707d3> ;
    skos:inScheme <> ;
    skos:notation "4.1.7" ;
    skos:prefLabel "unterscheiden pragmatische Textsorten und deren zentrale Funktionen (u. a. Bericht, Nachricht, Feature, Reportage [informierend]; Anleitung, Erklärvideo [instruierend]; Schulordnung [regulierend]; Kommentar, Buchkritik [überzeugend, argumentierend]),"@de .

<stdESA/5f827bed-ff22-4ee4-b96c-83689b450fae> a skos:Concept ;
    skos:broader <stdESA/7201b260-b8ae-4681-abd3-ce9803b22f80> ;
    skos:inScheme <> ;
    skos:notation "4.6.3" ;
    skos:prefLabel "unterscheiden Nutzungsformen digitaler Formate und Umgebungen und stellen Zusammenhänge zwischen Intentionen, Gestaltung und Wirkung her,"@de .

<stdESA/5ffed5c6-464d-4df5-b4f2-56e67229b998> a skos:Concept ;
    skos:broader <stdESA/26002dd8-6ee4-4892-89d7-8c171f41c223> ;
    skos:inScheme <> ;
    skos:notation "2.4.1" ;
    skos:prefLabel "sammeln Ideen, ordnen Informationen und fertigen eine Gliederung an, z. B. numerische Gliederung, Cluster, Mindmap,"@de .

<stdESA/5fff39ef-9f5e-409d-9863-bb4669606034> a skos:Concept ;
    skos:broader <stdESA/f0eb4a10-1c8f-4ea5-808c-f8565646fb61> ;
    skos:inScheme <> ;
    skos:notation "3.1.2" ;
    skos:prefLabel "verfügen über eine sichere Worterkennung (bei selteneren Wörtern mit Selbstkorrektur),"@de .

<stdESA/60baa489-54be-4dbf-bba5-7e62e357dd75> a skos:Concept ;
    skos:broader <stdESA/de5ef34d-4139-4021-9c57-f872668c4334> ;
    skos:inScheme <> ;
    skos:notation "2.2.7" ;
    skos:prefLabel "nutzen Rechtschreibhilfen (Wörterbuch und digitale Rechtschreibhilfe),"@de .

<stdESA/62ee9d62-75d6-40b7-b88b-d40d7b0972f6> a skos:Concept ;
    skos:broader <stdESA/eaad13ff-350d-49fc-a955-139420ddd0ba> ;
    skos:inScheme <> ;
    skos:notation "1.3.3" ;
    skos:prefLabel "formulieren in vertrauten und unvertrauten Situationen angemessen; gehen auf die Gesprächspartnerin bzw. den Gesprächspartner und die jeweilige Situation ein,"@de .

<stdESA/67f132d8-1103-42a0-ba63-26555cb3a143> a skos:Concept ;
    skos:broader <stdESA/b229c8b2-2099-4213-9eb4-8f54683fbe42> ;
    skos:inScheme <> ;
    skos:notation "2.6.2" ;
    skos:prefLabel "nutzen ausgewählte textsortenspezifische Kriterien für die Textüberprüfung und -überarbeitung,"@de .

<stdESA/69a82816-8386-42d7-b76b-af6ab016b9fc> a skos:Concept ;
    skos:broader <stdESA/27acf01e-82d3-4302-a65e-cd1db51f5f01> ;
    skos:inScheme <> ;
    skos:notation "1.4.2" ;
    skos:prefLabel "nutzen typische Werkzeuge für das Sprechen vor anderen, z. B. Tafel, Plakat, Moderationskarten, Präsentationssoftware, Whiteboard,"@de .

<stdESA/69b6af95-5a02-49d6-b022-6235dc95813e> a skos:Concept ;
    skos:broader <stdESA/de5ef34d-4139-4021-9c57-f872668c4334> ;
    skos:inScheme <> ;
    skos:notation "2.2.5" ;
    skos:prefLabel "kennen und nutzen die Interpunktionszeichen: Punkt, Komma, Fragezeichen, Ausrufezeichen, Anführungszeichen, Semikolon, Doppelpunkt, Auslassungspunkte, Gedankenstrich, Klammern,"@de .

<stdESA/6d1104c8-e304-4565-8a17-96a42fed366f> a skos:Concept ;
    skos:broader <stdESA/26002dd8-6ee4-4892-89d7-8c171f41c223> ;
    skos:inScheme <> ;
    skos:notation "2.4.9" ;
    skos:prefLabel "wenden Planungsstrategien (auch kooperativ) an und reflektieren sowohl ihren Planungsprozess als auch den Strategieeinsatz."@de .

<stdESA/7242bd81-1fc2-44bf-bd5a-2fae0a048761> a skos:Concept ;
    skos:broader <stdESA/4f4c0df7-077c-4f99-942c-5f7ed9e3b458> ;
    skos:inScheme <> ;
    skos:notation "3.3.1" ;
    skos:prefLabel "ermitteln explizite oder durch einfaches Schließen zu gewinnende Textinformationen auf lokaler Ebene und beachten dabei auch sprachliche Mittel zur Sicherung des Textzusammenhangs,"@de .

<stdESA/7372f39a-d0da-4f25-967b-8e5d85e11f93> a skos:Concept ;
    skos:broader <stdESA/a203e7dd-1858-4178-a9d4-cf3b6eae3b24> ;
    skos:inScheme <> ;
    skos:notation "1.2.2" ;
    skos:prefLabel "hören in vertrauten und unvertrauten Situationen sinnverstehend zu,"@de .

<stdESA/751a0f8d-c717-457b-bdcc-d2d24d083784> a skos:Concept ;
    skos:broader <stdESA/b229c8b2-2099-4213-9eb4-8f54683fbe42> ;
