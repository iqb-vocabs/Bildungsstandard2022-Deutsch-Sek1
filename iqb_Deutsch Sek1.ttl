    skos:prefLabel "Texte und Gespräche"@de .

<stdESA/d498425e-f3fb-458e-8cb6-b12202b1069d> a skos:Concept ;
    skos:broader <stdESA/58ff39b2-4511-46ad-a611-dd9bb0ae0da9> ;
    skos:inScheme <> ;
    skos:notation "5.2.4" ;
    skos:prefLabel "Aussprache"@de .

<stdESA/d650764e-7e73-413c-825d-5b7006a37662> a skos:Concept ;
