<stdMSA/1d5ea370-6d21-42bf-91df-4f4b1baafa05> a skos:Concept ;
    skos:broader <stdMSA/c632f39b-17dc-4016-9f04-580e0cf572a5> ;
    skos:inScheme <> ;
    skos:notation "5.1.3" ;
    skos:prefLabel "Ausprägungen und Bedingungen sprachlicher Variation und Vielfalt"@de .

<stdMSA/1d92df89-d861-4270-8f95-211249d96723> a skos:Concept ;
    skos:broader <stdMSA/859cffed-ece0-4089-873f-1bc4ae8b2cf8> ;
    skos:inScheme <> ;
    skos:notation "3.1.3" ;
    skos:prefLabel "lesen komplexere Texte sinngestaltend vor,"@de .

<stdMSA/2c0f3461-a016-455f-86b7-91532018d474> a skos:Concept ;
    skos:broader <stdMSA/89ede925-49ea-4461-a12c-3bc005169287> ;
    skos:inScheme <> ;
    skos:notation "2.4.6" ;
    skos:prefLabel "nutzen Informationsquellen gezielt, angemessen und reflektiert, insbesondere Bibliotheken, Nachschlagewerke, Zeitungen, Newsportale, digitale Suchdienste und (digitale) Ressourcen zur deutschen Sprache,"@de .

<stdMSA/2ef42025-cb0b-4065-a47c-cb9c46fb78a7> a skos:Concept ;
    skos:broader <stdMSA/ceba7d5b-1136-47f6-8c53-5c43831a9c44> ;
    skos:inScheme <> ;
    skos:notation "1.2.3" ;
