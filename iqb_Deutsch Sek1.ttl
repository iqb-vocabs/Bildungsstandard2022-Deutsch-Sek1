    skos:broader <stdESA/446c5c43-8c26-455c-9001-a8bb2e2066c6> ;
    skos:inScheme <> ;
    skos:notation "2.5.3" ;
    skos:prefLabel "realisieren mit unterschiedlichen Textsorten grundlegende kommunikative und personale Schreibfunktionen:"@de .

<stdESA/7ffce14b-ee2c-4eb9-aae3-3cba1330b9a9> a skos:Concept ;
    skos:broader <stdESA/27acf01e-82d3-4302-a65e-cd1db51f5f01> ;
    skos:inScheme <> ;
    skos:notation "1.4.1" ;
    skos:prefLabel "leisten längere, auch freie Beiträge adressatenorientiert und mit Hilfe eines Stichwortzettels/einer Gliederung/von Notizen/eines Textes, z. B. Referat, Präsentation, Rede, Gedicht,"@de .

<stdESA/8a6af37a-51cd-429b-9f5e-84c36787e5aa> a skos:Concept ;
    skos:broader <stdESA/eaad13ff-350d-49fc-a955-139420ddd0ba> ;
    skos:inScheme <> ;
    skos:notation "1.3.1" ;
    skos:prefLabel "beteiligen sich in vertrauten und unvertrauten Gesprächssituationen mit verständlichen, zuhörerbezogenen, themenbezogenen und konstruktiven Beiträgen,"@de .

<stdESA/8b81d254-828e-430c-932f-93e910f1c694> a skos:Concept ;
    skos:broader <stdESA/26002dd8-6ee4-4892-89d7-8c171f41c223> ;
