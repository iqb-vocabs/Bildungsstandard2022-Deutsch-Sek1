<stdMSA/1ba06afb-2101-4e78-9234-53b25c749b17> a skos:Concept ;
    skos:broader <stdMSA/a0fa590e-26e7-4603-a337-bfaf934b4761> ;
    skos:inScheme <> ;
    skos:notation "4.5.6" ;
    skos:prefLabel "beziehen Textaussagen auf eigene Welt- und Wertvorstellungen und überprüfen diese,"@de .

<stdMSA/1d3ba31b-ae12-472b-97e9-4c43460aad22> a skos:Concept ;
    skos:broader <stdMSA/d1128812-4c76-4715-a3a4-275006d3f761> ;
    skos:inScheme <> ;
