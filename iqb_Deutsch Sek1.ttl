    skos:inScheme <> ;
    skos:notation "1.1.2" ;
    skos:prefLabel "verfügen über einen angemessenen Wortschatz,"@de .

<stdESA/ecddadda-f650-4a9f-a94f-5176a00c3f1f> a skos:Concept ;
    skos:broader <stdESA/05945a07-990f-4430-bf2a-85a6c264ad91> ;
