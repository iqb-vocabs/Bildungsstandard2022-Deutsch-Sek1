@base <https://w3id.org/iqb/bista22des1/stdESA/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<00cc868e-cbe4-4f14-ae87-c391c4f393f0> a skos:Concept ;
    skos:broader <75cd0302-4b52-4cf3-8770-c84268188354> ;
    skos:inScheme <> ;
    skos:notation "1.1.8" ;
    skos:prefLabel "geben in vertrauten und unvertrauten Situationen nach vorgegebenen Kriterien gezielt Feedback zu Redebeiträgen,"@de .

<02c90fb5-5bb0-4afb-84ea-2627759b8b69> a skos:Concept ;
    skos:broader <d5558e46-feb6-4a89-ba94-75b51b546baa> ;
    skos:inScheme <> ;
    skos:notation "4.5.7" ;
    skos:prefLabel "ermitteln an ausgewählten Beispielen die Wirkungsabsichten von Texten und beurteilen diese."@de .

<0375eafd-ab6a-4028-9f75-acd017af63d1> a skos:Concept ;
    skos:broader <38bf2d37-1b37-417a-900c-f8ebfe669634> ;
    skos:inScheme <> ;
    skos:notation "2.5.5" ;
    skos:prefLabel "kennen und nutzen die Möglichkeiten digitaler Schreibwerkzeuge zur Formulierung und (formalen) Gestaltung von Texten, z. B. Schrift‐ und Textformatierung, Arrangieren und Re‐Arrangieren von Textbausteinen und Medienobjekten,"@de .

<0509e872-a2c5-40e7-b0dc-2ca26f05ca49> a skos:Concept ;
    skos:broader <58035696-91f5-4b10-a524-bfc474d06fd6> ;
    skos:inScheme <> ;
    skos:notation "5.1.3.3" ;
    skos:prefLabel "untersuchen ausgewählte Erscheinungen des Sprachwandels (z. B. Bedeutungswandel, Entlehnungen aus anderen Sprachen, Sprachwandel durch digitale Kommunikation),"@de .

<0bc7d2e0-d0a1-44c5-bdf8-11ac3d06153d> a skos:Concept ;
    skos:broader <2b5c702b-0d9c-4ea8-b436-ed92fbb5d2e7> ;
    skos:inScheme <> ;
    skos:notation "3.3.4" ;
    skos:prefLabel "verknüpfen Textinformationen, ziehen auch unter Nutzung ihres Vorwissens Schlussfolgerungen und konstruieren ein Gesamtverständnis."@de .

<0dd90e69-c705-4c72-a4ec-2b314f6986db> a skos:Concept ;
    skos:broader <c4e6462b-0905-4af1-bc19-c9f3be62e835> ;
    skos:inScheme <> ;
    skos:notation "4.1.7" ;
    skos:prefLabel "unterscheiden pragmatische Textsorten und deren zentrale Funktionen (u. a. Bericht, Nachricht, Feature, Reportage [informierend]; Anleitung, Erklärvideo [instruierend]; Schulordnung [regulierend]; Kommentar, Buchkritik [überzeugend, argumentierend]),"@de .

<10095a51-967e-4876-8ee9-32eb708abe6e> a skos:Concept ;
    skos:broader <7a928de1-6257-48ea-9e37-23e0752d8ba8> ;
    skos:inScheme <> ;
    skos:notation "1.3.4" ;
    skos:prefLabel "nehmen in Gesprächen verschiedene Rollen (z. B. Diskutantin bzw. Diskutant) ein,"@de .

<122e73a2-7114-4c66-ab72-d91310970967> a skos:Concept ;
    skos:broader <38bf2d37-1b37-417a-900c-f8ebfe669634> ;
    skos:inScheme <> ;
    skos:notation "2.5.7" ;
    skos:prefLabel "nehmen Gedanken und deutlich markierte Positionen anderer in den eigenen Text auf und positionieren sich zu ihnen,"@de .

<13afc739-bf35-415c-a36a-4be460c45b4d> a skos:Concept ;
    skos:broader <f1002003-0d2f-4a99-9814-3bdc4ab22a3f> ;
    skos:inScheme <> ;
    skos:notation "4.4.1" ;
    skos:prefLabel "nutzen zentrale Informationen zu Figuren, zu Raum- und Zeitdarstellung, Handlungs- und Konfliktverlauf sowie Atmosphäre zum Aufbau von Textverständnis und -deutung,"@de .

<16f5c5a3-e5db-4b96-bc1b-dd2f884e8e32> a skos:Concept ;
    skos:broader <38bf2d37-1b37-417a-900c-f8ebfe669634> ;
    skos:inScheme <> ;
    skos:notation "2.5.10" ;
    skos:prefLabel "kennen Formulierungsstrategien und wenden diese (auch kooperativ) an. Sie reflektieren ihren Strategieeinsatz."@de .

<17d39522-cc98-4cbf-b0cf-6d593caa7874> a skos:Concept ;
    skos:broader <c4e6462b-0905-4af1-bc19-c9f3be62e835> ;
    skos:inScheme <> ;
    skos:notation "4.1.2" ;
    skos:prefLabel "unterscheiden epische, lyrische und dramatische Texte, insbesondere epische Kleinformen (z. B. Fabel, Kurzgeschichte), Erzählung, (Jugend-)Roman, Gedichte, Drama,"@de .

<187942fd-28f7-4e90-96dd-b333ff5d0d6b> a skos:Concept ;
    skos:broader <6158d616-bbe8-49ad-b26c-3c9ff248dad7> ;
    skos:inScheme <> ;
    skos:notation "2.6.3.4" ;
    skos:prefLabel "unbekannte, unterschiedliche Adressaten,"@de .

<1a777240-6fb6-4834-934e-162c86d6dcd7> a skos:Concept ;
    skos:broader <75cd0302-4b52-4cf3-8770-c84268188354> ;
    skos:inScheme <> ;
    skos:notation "1.1.7" ;
    skos:prefLabel "wählen geeignete (multimodale) Medien mit Unterstützung sinnvoll aus und setzen diese gezielt ein, um den Inhalt und die Wirkung eigener Sprechbeiträge zu unterstützen,"@de .

<1cc6fdb3-c357-4ebe-b79c-882920c0a9d8> a skos:Concept ;
    skos:broader <23ff64ec-87ac-43f5-a194-896ce0d61e1e> ;
    skos:inScheme <> ;
    skos:notation "2.1.1" ;
    skos:prefLabel "schreiben Wörter, Wortgruppen und Sätze flüssig, d. h. schnell, sicher und korrekt (automatisiert), und formulieren ihre Gedanken weitgehend flüssig,"@de .

<1fb75d0a-b86b-4794-9c30-40bf9f7f4d4e> a skos:Concept ;
    skos:broader <2b5c702b-0d9c-4ea8-b436-ed92fbb5d2e7> ;
    skos:inScheme <> ;
    skos:notation "3.3.2" ;
    skos:prefLabel "stellen bei multimodalen Texten einfache Bezüge zwischen Text, Bild und Ton her,"@de .

<2019e64d-e5b5-40bc-8659-3b2b306c7a67> a skos:Concept ;
    skos:broader <c4e6462b-0905-4af1-bc19-c9f3be62e835> ;
    skos:inScheme <> ;
    skos:notation "4.1.6" ;
    skos:prefLabel "unterscheiden zentrale Textmuster (Erzählen, Berichten, Beschreiben, Argumentieren),"@de .

<20bc80e3-b8d6-400c-a272-a56c43306f2f> a skos:Concept ;
    skos:broader <b40314f0-b078-4b3b-b52a-05f733eb4311> ;
    skos:inScheme <> ;
    skos:notation "2.5.3.5" ;
    skos:prefLabel "Appellieren: Auffordern, Bitten, Wünschen, Begründen (Argumentieren), Erörtern, Überzeugen, z. B. in den Textsorten: Aufruf, Werbeanzeige, Stellungnahme, Antrag, Bewerbungsanschreiben, Buch-, Spiel- oder Filmkritik,"@de .

<222b2cf3-50cf-45e3-b3a0-da8a09744e4f> a skos:Concept ;
    skos:broader <c5276f76-7440-496d-a034-17301994bc9f> ;
    skos:definition "Die Schülerinnen und Schüler erschließen sich Texte unterschiedlicher medialer Form – auch als Grundlage für die mündliche und schriftliche Anschlusskommunikation."@de ;
    skos:inScheme <> ;
    skos:notation "4.3" ;
    skos:prefLabel "Texte in unterschiedlicher medialer Form erschließen und nutzen"@de .

<24da74be-a6d2-4b8f-8e73-d5e1c3016dc2> a skos:Concept ;
    skos:broader <f1002003-0d2f-4a99-9814-3bdc4ab22a3f> ;
    skos:inScheme <> ;
    skos:notation "4.4.11" ;
    skos:prefLabel "gestalten Texte unterschiedlicher medialer Form szenisch,"@de .

<29d2f2e4-9e70-4db3-9abc-0ec65ffdec63> a skos:Concept ;
    skos:broader <50b9a421-b5f5-4b6e-bff5-fe69cdc165fc> ;
    skos:inScheme <> ;
    skos:notation "2.4.2" ;
    skos:prefLabel "sammeln sprachliche und gestalterische Mittel: Wörter und Wortfelder, Formulierungen,"@de .

<2bd9d693-6231-48b4-8df2-a4d486a91a9f> a skos:Concept ;
    skos:broader <d5558e46-feb6-4a89-ba94-75b51b546baa> ;
    skos:inScheme <> ;
    skos:notation "4.5.5" ;
    skos:prefLabel "stellen an ausgewählten Beispielen Zusammenhänge zwischen Intentionen, Textmerkmalen, sprachlichen Gestaltungsmitteln und Wirkungen her,"@de .

<2c39e20f-9eef-4c55-9cf3-d2993abede9a> a skos:Concept ;
    skos:broader <4d84ead8-2aba-4f85-ba3d-81abfb43fbdf> ;
    skos:inScheme <> ;
    skos:notation "2.6.6" ;
    skos:prefLabel "kennen sowohl allgemeine als auch textsortenspezifische Überarbeitungsstrategien und wenden diese (auch kooperativ) an. Sie reflektieren sowohl ihren Überarbeitungsprozess als auch den Strategieeinsatz."@de .

<2c7ee16e-2da3-4892-99ed-9c56992fc3d3> a skos:Concept ;
    skos:broader <35bc9b6e-0ac7-4b94-9a23-1952a225c9d3> ;
    skos:inScheme <> ;
    skos:notation "4.6.1" ;
    skos:prefLabel "kennen medienspezifische Gestaltungsmittel und beschreiben an ausgewählten Beispielen ihre Wirkung,"@de .

<2d5e280d-6627-4036-909e-636a8515cf21> a skos:Concept ;
    skos:broader <6017320d-36a0-4ef9-9648-e048ba82ffdf> ;
    skos:inScheme <> ;
    skos:notation "3.4.3" ;
    skos:prefLabel "bereiten das Lesen nach (u. a. zentrale Textaussagen wiedergeben, wertende Einschätzung zum Text abgeben, anfängliche Erwartungen und Annahmen zur Textaussage prüfen und ggf. korrigieren, eigenen Wissenszuwachs prüfen)."@de .

<2f86e89e-41de-4e6a-9939-61a99b8ab9a8> a skos:Concept ;
    skos:broader <68660d81-9d90-42ee-a2e7-bdb1764d4686> ;
    skos:inScheme <> ;
    skos:notation "2.2.1.3" ;
    skos:prefLabel "prägen sich Merkwörter ein (z. B. Eigennamen, Ausnahmeschreibungen), "@de .

<313db9bf-4b46-45dc-9a00-e1af3f43a23b> a skos:Concept ;
    skos:broader <6158d616-bbe8-49ad-b26c-3c9ff248dad7> ;
    skos:inScheme <> ;
    skos:notation "2.6.3.1" ;
    skos:prefLabel "die Aufgabenstellung (Schreibsituation, Schreibanlass, Textfunktion und Textsorte) Aufbau, Inhalt und Formulierungen,"@de .

<31b113f5-60d9-4eb8-80ec-ad1d79cef4a1> a skos:Concept ;
    skos:broader <161fd7aa-41d2-4c28-b43a-aee961435408> ;
    skos:inScheme <> ;
    skos:notation "5.2.2.1" ;
    skos:prefLabel "beschreiben und vergleichen den Aufbau von Texten und Gesprächen strukturell und funktional unter Anwendung von Fachbegriffen (s. Übersicht: Grundlegende sprachliche Struktureinheiten) und ermitteln Gemeinsamkeiten und Unterschiede,"@de .

<31f51aec-df13-4df3-b1f0-4477e6b5697c> a skos:Concept ;
    skos:broader <58035696-91f5-4b10-a524-bfc474d06fd6> ;
    skos:inScheme <> ;
    skos:notation "5.1.3.1" ;
    skos:prefLabel "unterscheiden angeleitet und an ausgewählten Beispielen Ausprägungen von Sprache und Sprachvariation (z. B. Standardsprache – Alltagssprache, Fachsprache, Dialekt, Regionalsprache; Mehrsprachigkeit; Sprache in der digitalen Kommunikation; Deutsche Gebärdensprache) und reflektieren Verwendungsweisen,"@de .

<34734e40-0e14-407c-9557-d6703a04908d> a skos:Concept ;
    skos:broader <b5604218-e8e1-4d64-af8b-06e1b1dc7049> ;
    skos:inScheme <> ;
    skos:notation "1.2.1.2" ;
    skos:prefLabel "Verstehen und Nicht-Verstehen zum Ausdruck bringen, z. B. durch Nachfragen, durch Kommentare, "@de .

<366b4de3-aaec-427b-b3f9-805ee5143ec5> a skos:Concept ;
    skos:broader <54459a32-1634-47bf-8a7e-2b14e2c94a92> ;
    skos:inScheme <> ;
    skos:notation "5.1.1.1" ;
    skos:prefLabel "kennen Grundbedingungen gelingender Kommunikation und unterscheiden die Handlungs-, Inhalts- und Beziehungsebene,"@de .

<36c643df-74d1-4e85-ac75-2895fb4777a3> a skos:Concept ;
    skos:broader <d5558e46-feb6-4a89-ba94-75b51b546baa> ;
    skos:inScheme <> ;
    skos:notation "4.5.1" ;
    skos:prefLabel "erschließen aus einem oder mehreren klar strukturierten Texten zielgerichtet relevante Informationen, ordnen, selegieren und vergleichen diese und prüfen ihren Sachgehalt,"@de .

<37dc681d-5dd9-45bb-9e0e-0df84bb33674> a skos:Concept ;
    skos:broader <50b9a421-b5f5-4b6e-bff5-fe69cdc165fc> ;
    skos:inScheme <> ;
    skos:notation "2.4.6" ;
    skos:prefLabel "nutzen Informationsquellen gezielt und angemessen, insbesondere Bibliotheken, Nachschlagewerke, Zeitungen, Newsportale, digitale Suchdienste und (digitale) Ressourcen zur deutschen Sprache,"@de .

<38d7ce27-1508-4244-94dc-087874acb0bb> a skos:Concept ;
    skos:broader <35bc9b6e-0ac7-4b94-9a23-1952a225c9d3> ;
    skos:inScheme <> ;
    skos:notation "4.6.4" ;
    skos:prefLabel "untersuchen am Beispiel den Zusammenhang von digitalen Nutzerprofilen, Algorithmen, Gestaltung und Wirkung digitaler Angebote und bewerten ihn,"@de .

<39f03fee-4419-43a7-a9b4-312509623896> a skos:Concept ;
    skos:broader <b5604218-e8e1-4d64-af8b-06e1b1dc7049> ;
    skos:inScheme <> ;
    skos:notation "1.2.1.1" ;
    skos:prefLabel "Gesprächsprozesse verfolgen, verstehen und Inhalte für die Kommunikation nutzen,"@de .

<3a036be4-0eaf-4818-899d-28c6ea24b062> a skos:Concept ;
    skos:broader <f1002003-0d2f-4a99-9814-3bdc4ab22a3f> ;
    skos:inScheme <> ;
    skos:notation "4.4.10" ;
    skos:prefLabel "nutzen handlungs- und produktionsorientierte Verfahren, um ihre Deutung zu entwickeln und darzustellen und um Strukturen literarischer Texte nachzuvollziehen (z. B. innerer Monolog, Standbild, Parallelgedicht; Clip, Storyboard/ Fotostory),"@de .

<3a70fbfb-85d1-4a92-a0d0-1bfda5ab7ae1> a skos:Concept ;
    skos:broader <58035696-91f5-4b10-a524-bfc474d06fd6> ;
    skos:inScheme <> ;
    skos:notation "5.1.3.4" ;
    skos:prefLabel "vergleichen an ausgewählten Beispielen sprachliche Phänomene in verschiedenen Sprachen (u. a. Herkunftssprachen, Fremdsprachen), auch im Hinblick auf individuelle und gesellschaftliche Mehrsprachigkeit."@de .

<3c342f81-e94b-4ede-8c0c-b30c51ee1388> a skos:Concept ;
    skos:broader <38bf2d37-1b37-417a-900c-f8ebfe669634> ;
    skos:inScheme <> ;
    skos:notation "2.5.4" ;
    skos:prefLabel "nutzen unterschiedliche Schreibformen: kooperative (z. B. Schreibkonferenzen), produktive (z. B. Umschreiben, Weiterschreiben) und kreative (z. B. Schreiben zu Bildern),"@de .

<3f0630d4-bdb8-4ec9-83bd-cb4f1fbc70b2> a skos:Concept ;
    skos:broader <0280da36-3f78-4d9a-9dd6-6bf2758a9151> ;
    skos:inScheme <> ;
    skos:notation "5.2.1.5" ;
    skos:prefLabel "ermitteln an ausgewählten Beispielen den funktionalen Beitrag von Wörtern verschiedener Wortarten zum Aufbau von Sätzen, "@de .

<482365ed-8a23-4c11-80dd-777d4b8aade4> a skos:Concept ;
    skos:broader <21b9e2a9-e0f5-463d-8ace-af56ac97f58b> ;
    skos:inScheme <> ;
    skos:notation "2.2.5" ;
    skos:prefLabel "kennen und nutzen die Interpunktionszeichen: Punkt, Komma, Fragezeichen, Ausrufezeichen, Anführungszeichen, Semikolon, Doppelpunkt, Auslassungspunkte, Gedankenstrich, Klammern,"@de .

<486af035-8c97-4b17-a658-c9c96711f626> a skos:Concept ;
    skos:broader <f1002003-0d2f-4a99-9814-3bdc4ab22a3f> ;
    skos:inScheme <> ;
    skos:notation "4.4.5" ;
    skos:prefLabel "vergleichen Texte in Hinblick auf Themen und Gestaltung und nehmen begründete Wertungen vor (auch intermedial, z. B. Buch und Film),"@de .

<49c649ab-fd6d-4058-8958-24132ebaa4f7> a skos:Concept ;
    skos:broader <6158d616-bbe8-49ad-b26c-3c9ff248dad7> ;
    skos:inScheme <> ;
    skos:notation "2.6.3.3" ;
    skos:prefLabel "Kohärenz und Verständlichkeit,"@de .

<4e723264-3c2e-4b28-bdc6-42dc40a400f9> a skos:Concept ;
    skos:broader <75cd0302-4b52-4cf3-8770-c84268188354> ;
    skos:inScheme <> ;
    skos:notation "1.1.4" ;
    skos:prefLabel "nutzen ihre Vorbereitungen (z. B. Notizen) für die sach- und situationsgerechte Gestaltung vertrauter und unvertrauter Situationen, z. B. Bewerbungsgespräch, Diskussion, Präsentation,"@de .

<4eccfd15-43c6-458c-9dc9-1d418d198d59> a skos:Concept ;
    skos:broader <4d84ead8-2aba-4f85-ba3d-81abfb43fbdf> ;
    skos:inScheme <> ;
    skos:notation "2.6.1" ;
    skos:prefLabel "erkennen anhand vorgegebener Kriterienkataloge den Überarbeitungsbedarf von Texten, überlegen Schritte zur Textüberarbeitung und setzen diese um, "@de .

<500c9e73-2679-4241-affb-8caf6aec766d> a skos:Concept ;
    skos:broader <d5558e46-feb6-4a89-ba94-75b51b546baa> ;
    skos:inScheme <> ;
    skos:notation "4.5.4" ;
    skos:prefLabel "unterscheiden informierende, erzählende und appellierende Elemente in Texten,"@de .

<52eb0d23-cd08-4667-89f8-c4bd3b6f7b59> a skos:Concept ;
    skos:broader <38bf2d37-1b37-417a-900c-f8ebfe669634> ;
    skos:inScheme <> ;
    skos:notation "2.5.2" ;
    skos:prefLabel "formulieren Texte auch für unbekannte und unterschiedliche Adressaten angemessen, "@de .

<52f8db60-09f3-4ad6-9c0f-d5af81169361> a skos:Concept ;
    skos:broader <402ce841-8da6-473d-8354-afb14b2432d5> ;
    skos:inScheme <> ;
    skos:notation "1.2.2.3" ;
    skos:prefLabel "beurteilen Aussagen zum Gehörten begründet,"@de .

<55e05f94-2cde-4c85-ba40-9547bfc5c294> a skos:Concept ;
    skos:broader <38bf2d37-1b37-417a-900c-f8ebfe669634> ;
    skos:inScheme <> ;
    skos:notation "2.5.8" ;
    skos:prefLabel "formulieren eigene Positionen und begründen diese"@de .

<5701bce3-62b9-459e-8497-82d0e7b3f4d8> a skos:Concept ;
    skos:broader <22c7b269-b3ef-4dd6-8397-ad9d6fc12014> ;
    skos:inScheme <> ;
    skos:notation "5.1.2.1" ;
    skos:prefLabel "untersuchen monologische (z. B. Texte, Podcasts) und interaktionale Formen des Sprechens und des Schreibens (z. B. Gespräche, Chats) hinsichtlich ihrer Struktur und Organisation (ausgewählte Aspekte),"@de .

<571bdb3d-5715-4f41-a25c-d5f740556a57> a skos:Concept ;
    skos:broader <837374d8-d20c-4c8c-9469-10239cb22b79> ;
    skos:inScheme <> ;
    skos:notation "1.4.4" ;
    skos:prefLabel "planen und gestalten Formen des szenischen Sprechens und Spielens, z. B. szenische Lesung, Szene, Aufführung, Hörspiel, Videoclip, Standbild, Improvisation."@de .

<574a72fe-53f3-47bc-8787-771c3e980def> a skos:Concept ;
    skos:broader <b912ae89-9eff-46d6-bea8-d372ae33bc01> ;
    skos:definition "Die Schülerinnen und Schüler beachten bei der Planung und Strukturierung, beim Formulieren und beim Überarbeiten von Texten allgemeine Textmerkmale, Adressaten, den Schreibanlass, Schreibfunktion und Textsorte, Formen des Schreibens und den Einsatz digitaler Schreibwerkzeuge, den Umgang mit Quellen beim Belegen und Zitieren und die Anwendung von Schreibstrategien."@de ;
    skos:inScheme <> ;
    skos:notation "2.3" ;
    skos:prefLabel "Texte verfassen"@de .

<58cbd646-d58c-417b-8cfa-17cb61e01b24> a skos:Concept ;
    skos:broader <38bf2d37-1b37-417a-900c-f8ebfe669634> ;
    skos:inScheme <> ;
    skos:notation "2.5.9" ;
    skos:prefLabel "integrieren Textbelege und andere Quellen formal richtig in den eigenen Text (zitieren),"@de .

<5caa8223-ddcd-498f-b702-708f9428a6b7> a skos:Concept ;
    skos:broader <7a928de1-6257-48ea-9e37-23e0752d8ba8> ;
    skos:inScheme <> ;
    skos:notation "1.3.7" ;
    skos:prefLabel "sprechen über das sprachliche und literarische Lernen sowie über Lernerfahrungen: geben Beobachtungen wieder, beschreiben Sachverhalte, geben Erklärungen und Begründungen, reflektieren."@de .

<5ddc5b5a-4fa3-45f8-b340-dd18b3b275b0> a skos:Concept ;
    skos:broader <a96afc32-a9c3-485a-9225-7b300b8028a4> ;
    skos:inScheme <> ;
    skos:notation "1.1.3.4" ;
    skos:prefLabel "erläutern,"@de .

<5ef786f2-8308-4151-b5de-daa241ff7dfb> a skos:Concept ;
    skos:broader <22c7b269-b3ef-4dd6-8397-ad9d6fc12014> ;
    skos:inScheme <> ;
    skos:notation "5.1.2.2" ;
    skos:prefLabel "untersuchen die Anforderungen an die Rollen von Sprecher/Schreiber – Hörer/Leser im Hinblick auf gelingende Kommunikation und werden diesen im eigenen sprachlichen Handeln gerecht,"@de .

<5fedea0e-1e34-4b19-820a-5a9e14344980> a skos:Concept ;
    skos:broader <f3136ad6-8044-4ec4-8ca8-b03ed674f368> ;
    skos:inScheme <> ;
    skos:notation "3.2.1" ;
    skos:prefLabel "orientieren sich in und zwischen Texten unterschiedlicher Komplexität und verschaffen sich auch mithilfe textueller Lesehilfen (u. a. Überschriften, Zwischenüberschriften, Absätze, Abbildungen, Illustrationen) einen Überblick: überfliegendes, selegierendes, navigierendes Lesen."@de .

<60359b06-253d-4105-9241-ee1c3772c840> a skos:Concept ;
    skos:broader <75cd0302-4b52-4cf3-8770-c84268188354> ;
    skos:inScheme <> ;
    skos:notation "1.1.5" ;
    skos:prefLabel "bauen Redebeiträge in vertrauten und unvertrauten Situationen für andere nachvollziehbar und sinnvoll auf,"@de .

<6140d957-476e-4bc8-a2f1-981007595af9> a skos:Concept ;
    skos:broader <2b5c702b-0d9c-4ea8-b436-ed92fbb5d2e7> ;
    skos:inScheme <> ;
    skos:notation "3.3.1" ;
    skos:prefLabel "ermitteln explizite oder durch einfaches Schließen zu gewinnende Textinformationen auf lokaler Ebene und beachten dabei auch sprachliche Mittel zur Sicherung des Textzusammenhangs,"@de .

<648c177c-9c53-40f1-9efb-5c17cdb5daf0> a skos:Concept ;
    skos:broader <50b9a421-b5f5-4b6e-bff5-fe69cdc165fc> ;
    skos:inScheme <> ;
    skos:notation "2.4.4" ;
    skos:prefLabel "klären entsprechend dem Schreibziel die Schreibabsicht, Schreibsituation und Textfunktion,"@de .

<65013396-a4c7-4246-9357-ff8596f41b1a> a skos:Concept ;
    skos:broader <7a928de1-6257-48ea-9e37-23e0752d8ba8> ;
    skos:inScheme <> ;
    skos:notation "1.3.5" ;
    skos:prefLabel "praktizieren ausgewählte Gesprächs- und Lernformen, z. B. literarische Gespräche, Rollenspiele, Vorstellungsgespräche, Diskussionen, videogestützte Gespräche, Interviews,"@de .

<65511534-3b94-4a37-8931-0d6840f5989d> a skos:Concept ;
    skos:broader <837374d8-d20c-4c8c-9469-10239cb22b79> ;
    skos:inScheme <> ;
    skos:notation "1.4.1" ;
    skos:prefLabel "leisten längere, auch freie Beiträge adressatenorientiert und mit Hilfe eines Stichwortzettels/einer Gliederung/von Notizen/eines Textes, z. B. Referat, Präsentation, Rede, Gedicht,"@de .

<660f9f0b-81aa-48cc-b81f-5f178eb6ac93> a skos:Concept ;
    skos:broader <f1002003-0d2f-4a99-9814-3bdc4ab22a3f> ;
    skos:inScheme <> ;
    skos:notation "4.4.4" ;
    skos:prefLabel "formulieren Wertungen von Texten und begründen sie,"@de .

<6964703b-4791-43d3-a24f-5beae2d3b15a> a skos:Concept ;
    skos:broader <6158d616-bbe8-49ad-b26c-3c9ff248dad7> ;
    skos:inScheme <> ;
    skos:notation "2.6.3.2" ;
    skos:prefLabel "Angemessenheit sprachlicher Gestaltungsmittel, Rechtschreibung und Grammatik,"@de .

<6a351609-53af-45aa-8fd9-f5287d31544c> a skos:Concept ;
    skos:broader <161fd7aa-41d2-4c28-b43a-aee961435408> ;
    skos:inScheme <> ;
    skos:notation "5.2.2.2" ;
    skos:prefLabel "nutzen ihr Wissen zu sprachlichen Gestaltungsmitteln für die Beschreibung und Untersuchung von (auch literarischen) Texten, Reden, Gesprächen und Formen digitaler Kommunikation."@de .

<6b6818ff-2fb9-4f82-8803-f00cecd26ef4> a skos:Concept ;
    skos:broader <8d07cde9-e806-410d-832e-792b1a55fe25> ;
    skos:inScheme <> ;
    skos:notation "1.2.3" ;
    skos:prefLabel "wählen aus grundlegenden kognitiven und metakognitiven Strategien aus, die sie vor, während und nach dem Zuhören einsetzen, z. B. die Erwartungshaltung zielorientiert steuern, Aufmerksamkeit steuern, Informationen sichern und zusammenfassen: Notizen, Protokoll."@de .

<6c87ea0f-f367-4e5a-b5fc-b3be7d4ddd77> a skos:Concept ;
    skos:broader <a96afc32-a9c3-485a-9225-7b300b8028a4> ;
    skos:inScheme <> ;
    skos:notation "1.1.3.6" ;
    skos:prefLabel "appellieren,"@de .

<6f7a58e5-03e7-4ec2-95b9-9ca817cf1a9e> a skos:Concept ;
    skos:broader <837374d8-d20c-4c8c-9469-10239cb22b79> ;
    skos:inScheme <> ;
    skos:notation "1.4.2" ;
    skos:prefLabel "nutzen typische Werkzeuge für das Sprechen vor anderen, z. B. Tafel, Plakat, Moderationskarten, Präsentationssoftware, Whiteboard,"@de .

<70d79fbd-d094-4aa7-99da-377c830b1cd3> a skos:Concept ;
    skos:broader <50b9a421-b5f5-4b6e-bff5-fe69cdc165fc> ;
    skos:inScheme <> ;
    skos:notation "2.4.8" ;
    skos:prefLabel "nutzen die Möglichkeiten digitaler Schreibwerkzeuge zur Vorbereitung der Textproduktion, z. B. Anfertigen von Notizen und Skizzen, Anlegen von Stoffsammlungen, Exzerpieren aus digitalen Informationsressourcen,"@de .

<7117c7b1-6fbe-4a9b-9ec4-0fb71e7733fb> a skos:Concept ;
    skos:broader <58035696-91f5-4b10-a524-bfc474d06fd6> ;
    skos:inScheme <> ;
    skos:notation "5.1.3.2" ;
    skos:prefLabel "unterscheiden informelle und elaborierte Mündlichkeit/Schriftlichkeit und können an konkreten Beispielen Wirkungen einschätzen,"@de .

<714814ab-2e62-4c54-8a78-cfb70818bd6b> a skos:Concept ;
    skos:broader <0280da36-3f78-4d9a-9dd6-6bf2758a9151> ;
    skos:inScheme <> ;
    skos:notation "5.2.1.2" ;
    skos:prefLabel "nutzen Wörter, Wendungen und Formulierungsmuster funktional und erweitern ihren Wortschatz,"@de .

<720f3b4f-a62a-40be-96e8-314ed3819cd7> a skos:Concept ;
    skos:broader <b40314f0-b078-4b3b-b52a-05f733eb4311> ;
    skos:inScheme <> ;
    skos:notation "2.5.3.3" ;
    skos:prefLabel "Darstellen: Informieren auf Basis klar strukturierter Material- und Textquellen (informierend materialgestützt schreiben)"@de .

<72805a24-fe05-4adb-a722-45ed5be23775> a skos:Concept ;
    skos:broader <c4e6462b-0905-4af1-bc19-c9f3be62e835> ;
    skos:inScheme <> ;
    skos:notation "4.1.8" ;
    skos:prefLabel "nutzen Angaben zum Wirklichkeitsbezug von Texten als Rezeptionshinweise (z. B. Gattungs-/Genrebezeichnungen: Roman, Satire, Krimi, Biopic, Dokumentation, Doku-Drama),"@de .

<759d5a59-947f-436e-b940-50da863e7cb6> a skos:Concept ;
    skos:broader <21b9e2a9-e0f5-463d-8ace-af56ac97f58b> ;
    skos:inScheme <> ;
    skos:notation "2.2.2" ;
    skos:prefLabel "schreiben Kernwörter richtig (Inhalts- und Funktionswörter)"@de .

<76dcc44f-1a27-4991-bb4f-1dcd5bf52421> a skos:Concept ;
    skos:broader <0280da36-3f78-4d9a-9dd6-6bf2758a9151> ;
    skos:inScheme <> ;
    skos:notation "5.2.1.3" ;
    skos:prefLabel "nutzen grammatische Operationen und Fachbegriffe für die Untersuchung sprachlicher Struktureinheiten (s. Übersicht: Verzeichnis grundlegender grammatischer Fachausdrücke),"@de .

<780818e3-1245-4ded-a298-ef2aeff075ab> a skos:Concept ;
    skos:broader <27f070fa-bb81-4a63-b008-fb9e532d3deb> ;
    skos:inScheme <> ;
    skos:notation "4.2.2" ;
    skos:prefLabel "bewerten Rechercheergebnisse (insbes. Internet) im Hinblick auf ihre thematische Relevanz und Verlässlichkeit (Informationsangebote vergleichen, Quellenangaben und -qualität prüfen, Informationen zu Autorinnen und Autoren recherchieren, mögliche Textfunktionen beachten)."@de .

<7f50ad0b-c68e-4607-9e7c-2a035ad71382> a skos:Concept ;
    skos:broader <54459a32-1634-47bf-8a7e-2b14e2c94a92> ;
    skos:inScheme <> ;
    skos:notation "5.1.1.3" ;
    skos:prefLabel "untersuchen an ausgewählten Beispielen Normen demokratischer Gesprächs- und Kommunikationskultur und sprachlicher Höflichkeit (auch in digitalen Umgebungen) und nutzen diese für die sprachliche Gestaltung eigener Äußerungen sowie für den Umgang mit den sprachlichen Äußerungen anderer,"@de .

<8105e21f-5abe-405a-9e59-9eaddfdbb1ba> a skos:Concept ;
    skos:broader <0280da36-3f78-4d9a-9dd6-6bf2758a9151> ;
    skos:inScheme <> ;
    skos:notation "5.2.1.1" ;
    skos:prefLabel "untersuchen Wörter und Wendungen in ihrer Struktur und hinsichtlich ihrer Verwendungsbedingungen, ihrer Bedeutung und ihrer Beziehungen zu anderen Wörtern,"@de .

<8197e8bf-54d8-4e84-bbd2-2e94e7ddc149> a skos:Concept ;
    skos:broader <7a928de1-6257-48ea-9e37-23e0752d8ba8> ;
    skos:inScheme <> ;
    skos:notation "1.3.6" ;
    skos:prefLabel "wenden allgemeine und für Gesprächs- und Lernformen spezifische Strategien an, z. B. Gespräche steuern (Diskussionen), zielführende Rückmeldungen geben (Feedback),"@de .

<82d4b8e3-2649-4837-998b-d72ae9bfa419> a skos:Concept ;
    skos:broader <21b9e2a9-e0f5-463d-8ace-af56ac97f58b> ;
    skos:inScheme <> ;
    skos:notation "2.2.7" ;
    skos:prefLabel "nutzen Rechtschreibhilfen (Wörterbuch und digitale Rechtschreibhilfe),"@de .

<87b00bf2-e98e-46bf-b17e-83b44c9a9b11> a skos:Concept ;
    skos:broader <d5558e46-feb6-4a89-ba94-75b51b546baa> ;
    skos:inScheme <> ;
    skos:notation "4.5.6" ;
    skos:prefLabel "beziehen an ausgewählten Beispielen Textaussagen auf eigene Welt- und Wertvorstellungen und überprüfen diese,"@de .

<88113829-61bc-4e43-90ba-e40b3c8c8a70> a skos:Concept ;
    skos:broader <f1002003-0d2f-4a99-9814-3bdc4ab22a3f> ;
    skos:inScheme <> ;
    skos:notation "4.4.9" ;
    skos:prefLabel "tauschen sich mit anderen zu Text- bzw. Medienwahrnehmungen, -deutungen und -wertungen aus (u. a. literarisches Gespräch),"@de .

<894965fa-8f06-4250-9714-18275aa61669> a skos:Concept ;
    skos:broader <7a928de1-6257-48ea-9e37-23e0752d8ba8> ;
    skos:inScheme <> ;
    skos:notation "1.3.1" ;
    skos:prefLabel "beteiligen sich in vertrauten und unvertrauten Gesprächssituationen mit verständlichen, zuhörerbezogenen, themenbezogenen und konstruktiven Beiträgen,"@de .

<89748323-eb92-4da6-ac5e-4dc90a0a7b8e> a skos:Concept ;
    skos:broader <b40314f0-b078-4b3b-b52a-05f733eb4311> ;
    skos:inScheme <> ;
    skos:notation "2.5.3.6" ;
    skos:prefLabel "Appellieren: Argumentieren auf Basis klar strukturierter Material- und Textquellen (argumentierend materialgestützt schreiben)"@de .

<89e0f3e4-8133-4cc5-a3e6-8548a291134d> a skos:Concept ;
    skos:broader <4d84ead8-2aba-4f85-ba3d-81abfb43fbdf> ;
    skos:inScheme <> ;
    skos:notation "2.6.4" ;
    skos:prefLabel "holen Text-Feedback ein und formulieren selbst auch Feedback zu Texten anderer, "@de .

<8abc2d4a-1a9d-4b50-913c-6becb4cff913> a skos:Concept ;
    skos:broader <68660d81-9d90-42ee-a2e7-bdb1764d4686> ;
    skos:inScheme <> ;
    skos:notation "2.2.1.2" ;
    skos:prefLabel "erschließen sich die Struktur von Kernwörtern, z. B. gliedern in Silben und Wortbestandteile, verlängern, flektieren, ableiten, erweitern, "@de .

<8bf21d6f-5c33-4345-a6eb-f4e0da0dd8c3> a skos:Concept ;
    skos:broader <50b9a421-b5f5-4b6e-bff5-fe69cdc165fc> ;
    skos:inScheme <> ;
    skos:notation "2.4.7" ;
    skos:prefLabel "planen Texte kooperativ und strukturieren diese: z. B. Textbaupläne,"@de .

<8e29b4f0-8940-47db-a63e-7f6da724956e> a skos:Concept ;
    skos:broader <a96afc32-a9c3-485a-9225-7b300b8028a4> ;
    skos:inScheme <> ;
    skos:notation "1.1.3.1" ;
    skos:prefLabel "erzählen,"@de .

<8e58bbc2-c9dc-408a-b913-09f74c02b0bd> a skos:Concept ;
    skos:broader <21b9e2a9-e0f5-463d-8ace-af56ac97f58b> ;
    skos:inScheme <> ;
    skos:notation "2.2.8" ;
    skos:prefLabel "überprüfen und korrigieren Texte auf orthografische Richtigkeit."@de .

<8f1d5ba1-ff00-45b0-a122-5cd2a390c386> a skos:Concept ;
    skos:broader <23ff64ec-87ac-43f5-a194-896ce0d61e1e> ;
    skos:inScheme <> ;
    skos:notation "2.1.3" ;
    skos:prefLabel "gestalten Texte (handschriftlich und mithilfe digitaler Schreibwerkzeuge) übersichtlich und zielorientiert, bauen sie sinnvoll auf und strukturieren diese: z. B. Blattaufteilung, Seitenränder, Absatzstrukturen, Textformatierung und Layout."@de .

<93637534-243a-42d3-9251-6d6ffd90a7f7> a skos:Concept ;
    skos:broader <6017320d-36a0-4ef9-9648-e048ba82ffdf> ;
    skos:inScheme <> ;
    skos:notation "3.4.1" ;
    skos:prefLabel "bereiten das Lesen vor (u. a. Leseziel formulieren, Vorwissen aktivieren, Erwartungen an den Text formulieren, nach Möglichkeit die Textsorte klären), "@de .

<995a361a-3523-4a89-b7d8-5d1cd2d73129> a skos:Concept ;
    skos:broader <734f2b48-e4ae-4375-b060-21cf29a4d5f1> ;
    skos:inScheme <> ;
    skos:notation "3.1.1" ;
    skos:prefLabel "nutzen die schriftsprachlichen Informationen (u. a. Silbe, Morphem, Komma, Punkt, Fragezeichen, Großschreibung) beim Lesen,"@de .

<9977f7ba-efc0-4e8a-90e5-16c638107ae5> a skos:Concept ;
    skos:broader <402ce841-8da6-473d-8354-afb14b2432d5> ;
    skos:inScheme <> ;
    skos:notation "1.2.2.1" ;
    skos:prefLabel "geben Informationen wieder und ziehen einfache Schlussfolgerungen,"@de .

<99d9b73a-ab71-48dc-bcaa-8b428ad5c79d> a skos:Concept ;
    skos:broader <c4e6462b-0905-4af1-bc19-c9f3be62e835> ;
    skos:inScheme <> ;
    skos:notation "4.1.5" ;
    skos:prefLabel "kennen häufig verwendete sprachliche Gestaltungsmittel (u. a. Wortwahl, Wiederholung, sprachliche Bilder, Vergleich) und beschreiben an ausgewählten Beispielen ihre Wirkung,"@de .

<9d918410-929a-4163-8160-22c665cfd5c2> a skos:Concept ;
    skos:broader <f1002003-0d2f-4a99-9814-3bdc4ab22a3f> ;
    skos:inScheme <> ;
    skos:notation "4.4.6" ;
    skos:prefLabel "vergleichen Handlungen und Handlungsmotive von Figuren anhand ausgewählter Kriterien mit vertrauten Handlungen und Handlungsmotiven und nehmen begründete Wertungen vor,"@de .

<a2d06648-ea33-49c9-8ecc-b809f4da8e96> a skos:Concept ;
    skos:broader <b40314f0-b078-4b3b-b52a-05f733eb4311> ;
    skos:inScheme <> ;
    skos:notation "2.5.3.2" ;
    skos:prefLabel "Darstellen: Informieren (Beschreiben, Berichten), Erklären, Instruieren, Erörtern, z. B. in den Textsorten: Orts‐, Funktions‐ oder Bildbeschreibung, Bericht, Protokoll, Nachricht, (Online)Lexikon-Eintrag, Artikel für Schülerzeitung, Gebrauchsanweisung, Konstruktions- oder Spielanleitung, Zusammenfassung,"@de .

<a40eff8f-ad77-4a60-b16b-db9ac9fbe1d6> a skos:Concept ;
    skos:broader <68660d81-9d90-42ee-a2e7-bdb1764d4686> ;
    skos:inScheme <> ;
    skos:notation "2.2.1.4" ;
    skos:prefLabel "unterscheiden zwischen Kernwörtern und Fremdwörtern,"@de .

<a4bc1616-8611-40e6-8417-a26e2407fc35> a skos:Concept ;
    skos:broader <d5558e46-feb6-4a89-ba94-75b51b546baa> ;
    skos:inScheme <> ;
    skos:notation "4.5.2" ;
    skos:prefLabel "werten nichtlineare und multimodale Texte (Text-Bild-Bezüge) zielorientiert aus, z. B. um über ein Thema zu sprechen oder zu schreiben"@de .

<a5a5bf92-5143-4cc2-b525-6d958dd29fff> a skos:Concept ;
    skos:broader <837374d8-d20c-4c8c-9469-10239cb22b79> ;
    skos:inScheme <> ;
    skos:notation "1.4.3" ;
    skos:prefLabel "planen und gestalten typische Formen des Präsentierens, z. B. Standbild, Stegreif-Rede, Kurzreferat, Podcasts, Erklärvideos,"@de .

<a6405631-c41c-49cd-b9a8-6bbf7da3fe9f> a skos:Concept ;
    skos:broader <734f2b48-e4ae-4375-b060-21cf29a4d5f1> ;
    skos:inScheme <> ;
    skos:notation "3.1.3" ;
    skos:prefLabel "lesen komplexere Texte nach Vorbereitung sinngestaltend vor,"@de .

<a76b29cb-c0a7-476e-bd20-14f15a9f582a> a skos:Concept ;
    skos:broader <22c7b269-b3ef-4dd6-8397-ad9d6fc12014> ;
    skos:inScheme <> ;
    skos:notation "5.1.2.3" ;
    skos:prefLabel "unterscheiden an ausgewählten Beispielen grundlegende Textfunktionen – Darstellen (z. B. Informieren in einer Zeitungsmeldung), Appellieren (z. B. Überzeugen in einer Werbeanzeige; Gesetzestext), Ausdrücken (z. B. sich selbst ausdrücken in einem Bewerbungsschreiben, einer Beschwerde oder einem Gedicht) – und nutzen ihr Wissen über Textfunktionen für das eigene Schreiben und für die Erschließung von Texten."@de .

<a7a3fc99-927e-440f-99ab-b4f5b8ec8810> a skos:Concept ;
    skos:broader <c4e6462b-0905-4af1-bc19-c9f3be62e835> ;
    skos:inScheme <> ;
    skos:notation "4.1.4" ;
    skos:prefLabel "stellen an einem Beispiel Zusammenhänge zwischen Text und Kontext her (insbes. Entstehungszeit, Leben des Autors/der Autorin),"@de .

<abd3140e-7d61-4deb-bb5b-3933ca08e3f5> a skos:Concept ;
    skos:broader <d5558e46-feb6-4a89-ba94-75b51b546baa> ;
    skos:inScheme <> ;
    skos:notation "4.5.3" ;
    skos:prefLabel "unterscheiden Information und Wertung in Texten,"@de .

<ad390051-6481-42c3-a585-b9ba73ff9d95> a skos:Concept ;
    skos:broader <75cd0302-4b52-4cf3-8770-c84268188354> ;
    skos:inScheme <> ;
    skos:notation "1.1.9" ;
    skos:prefLabel "nutzen technische Möglichkeiten beim digitalen Kommunizieren in vertrauten und unvertrauten Situationen."@de .

<b170a110-fe7d-4273-a139-3113cab6c17a> a skos:Concept ;
    skos:broader <21b9e2a9-e0f5-463d-8ace-af56ac97f58b> ;
    skos:inScheme <> ;
    skos:notation "2.2.6" ;
    skos:prefLabel "verfügen über Fehleraufmerksamkeit und nutzen Strategien zur Klärung orthografischer Fragen,"@de .

<b3392947-b2af-49df-b5fc-6967431083f7> a skos:Concept ;
    skos:broader <50b9a421-b5f5-4b6e-bff5-fe69cdc165fc> ;
    skos:inScheme <> ;
    skos:notation "2.4.5" ;
    skos:prefLabel "entwickeln gemäß der Schreibaufgabe und der Zeitvorgabe einen Schreibplan, entscheiden sich für die angemessene Textsorte und konzipieren einen ziel-, adressaten- und situationsbezogenen Text,"@de .

