        <6df9f5ab-9eaf-407e-a39b-5df7827682cf>,
        <dcdd4437-4539-4ec7-a06f-7a65be0078e6> ;
    skos:notation "4.6" ;
    skos:prefLabel "Digitale Formate und Umgebungen"@de .

