@base <https://w3id.org/iqb/bista22des1/stdMSA/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<0237fe67-e7af-4fd8-bb9c-186ecc08fc50> a skos:Concept ;
    skos:broader <3cec36e7-400e-4804-8d6d-372991cc608e> ;
    skos:inScheme <> ;
    skos:notation "4.1.7" ;
    skos:prefLabel "unterscheiden pragmatische Textsorten und deren zentrale Funktionen (u. a. Bericht, Nachricht, Feature, Reportage [informierend]; Anleitung, Erklärvideo [instruierend]; Schulordnung [regulierend]; Kommentar, Buchkritik [überzeugend, argumentierend]),"@de .

<0387f33f-2997-4e8c-b0bd-4ccf9c6f7fdd> a skos:Concept ;
    skos:broader <6aa6a0a9-3e2b-4f60-961e-e558121b1701> ;
    skos:inScheme <> ;
    skos:notation "4.5.7" ;
    skos:prefLabel "ermitteln und beurteilen die Wirkungsabsichten von Texten."@de .

<03c18873-d249-46ba-b3c2-bfa84111091f> a skos:Concept ;
    skos:broader <a623de09-bab3-4dcd-b741-f095e46660df> ;
    skos:inScheme <> ;
    skos:notation "1.3.4" ;
    skos:prefLabel "nehmen in Gesprächen verschiedene Rollen (z. B. Moderation) ein und richten diese an den jeweiligen Zielen aus,"@de .

<05a70a48-7b46-405d-9f58-d84d702bbcd5> a skos:Concept ;
    skos:broader <f6b6aa3d-8c6e-4c4c-a10b-6e87527ed962> ;
    skos:inScheme <> ;
    skos:notation "1.1.1" ;
    skos:prefLabel "äußern sich verständlich und situationsadäquat auch in der gesprochenen Standardsprache,"@de .

<0a5fdfef-370e-4742-b9da-5a76c1f53ae2> a skos:Concept ;
    skos:broader <9a5651fb-a7d4-4d9e-9fd6-16f18c30d689> ;
    skos:inScheme <> ;
    skos:notation "2.5.6" ;
    skos:prefLabel "erstellen Schaubilder, Diagramme und Tabellen und integrieren diese sprachlich in den eigenen – auch digitalen – Text,"@de .

<0af208a6-e475-4d18-81c9-7ab669dec23a> a skos:Concept ;
    skos:broader <3cec36e7-400e-4804-8d6d-372991cc608e> ;
    skos:inScheme <> ;
    skos:notation "4.1.8" ;
    skos:prefLabel "nutzen Angaben zum Wirklichkeitsbezug von Texten und Medien als Rezeptionshinweise (z. B. Gattungs-/Genrebezeichnungen: Roman, Satire, Krimi, Biopic, Dokumentation, Doku-Drama) und setzen sich mit Beziehungen zwischen Wirklichkeit und Fiktion auseinander,"@de .

<0afa8cc3-5ca0-4815-bc2d-50524e9c706f> a skos:Concept ;
    skos:broader <256b3d49-fc3b-440d-b297-33f774091001> ;
    skos:inScheme <> ;
    skos:notation "5.1.2" ;
    skos:prefLabel "Mündliche und schriftliche Kommunikation"@de .

<0fbb578d-0ca0-4587-846a-b90cee2d4754> a skos:Concept ;
    skos:broader <eec3cd54-e901-445c-b03f-f95cd3071281> ;
    skos:inScheme <> ;
    skos:notation "2.2.5" ;
    skos:prefLabel "kennen und nutzen die Interpunktionszeichen: Punkt, Komma, Fragezeichen, Ausrufezeichen, Anführungszeichen, Semikolon, Doppelpunkt, Auslassungspunkte, Gedankenstrich, Klammern,"@de .

<1360c74f-d652-4b66-b08f-0e16854873c3> a skos:Concept ;
    skos:broader <6aa6a0a9-3e2b-4f60-961e-e558121b1701> ;
    skos:inScheme <> ;
    skos:notation "4.5.5" ;
    skos:prefLabel "stellen Zusammenhänge zwischen Intentionen, Textmerkmalen, sprachlichen Gestaltungsmitteln und Wirkungen her,"@de .

<15d053d4-e26c-42ba-a8df-0361c843e2a0> a skos:Concept ;
    skos:broader <fdb78af2-b1fb-4554-a1ff-b88c9ae8a7af> ;
    skos:definition "Die Schülerinnen und Schüler beachten bei der Planung und Strukturierung, beim Formulieren und beim Überarbeiten von Texten allgemeine Textmerkmale, Adressaten, den Schreibanlass, Schreibfunktion und Textsorte, Formen des Schreibens und den Einsatz digitaler Schreibwerkzeuge, den Umgang mit Quellen beim Belegen und Zitieren und die Anwendung von Schreibstrategien."@de ;
    skos:inScheme <> ;
    skos:notation "2.3" ;
    skos:prefLabel "Texte verfassen"@de .

<1857c257-005a-4ecf-b8a5-657e7b73c69f> a skos:Concept ;
    skos:broader <d72f4980-ad03-45e9-a022-1db9c07af1ec> ;
    skos:definition "Die Schülerinnen und Schüler erschließen sich ein Spektrum von Texten unterschiedlicher medialer Form und Komplexität – auch als Grundlage für die mündliche und schriftliche Anschlusskommunikation."@de ;
    skos:inScheme <> ;
    skos:notation "4.3" ;
    skos:prefLabel "Texte in unterschiedlicher medialer Form erschließen und nutzen"@de .

<1979ec46-5e1c-4148-a8f4-38cbd9e3093e> a skos:Concept ;
    skos:broader <f6b6aa3d-8c6e-4c4c-a10b-6e87527ed962> ;
    skos:inScheme <> ;
    skos:notation "1.1.4" ;
    skos:prefLabel "nutzen ihre Vorbereitungen (z. B. Notizen) für die sach- und situationsgerechte, differenzierte Gestaltung vertrauter und unvertrauter Situationen, z. B. Bewerbungsgespräch, Diskussion, Debatte, Präsentation,"@de .

<19ecf0d0-76fd-469e-b5b1-4e292acb44fb> a skos:Concept ;
    skos:broader <a623de09-bab3-4dcd-b741-f095e46660df> ;
    skos:inScheme <> ;
    skos:notation "1.3.2" ;
    skos:prefLabel "beachten gemeinsam entwickelte oder für öffentliche Kommunikation prototypische Gesprächsregeln; gestalten eine wertschätzende Gesprächsatmosphäre,"@de .

<1c12459f-1040-4b68-9062-9c6842b5dd6f> a skos:Concept ;
    skos:broader <9a5651fb-a7d4-4d9e-9fd6-16f18c30d689> ;
    skos:inScheme <> ;
    skos:notation "2.5.7" ;
    skos:prefLabel "nehmen Gedanken und deutlich markierte Positionen anderer in den eigenen Text auf und positionieren sich zu ihnen,"@de .

<1cba53a9-e2c3-4e83-a2b1-6ae67a973c60> a skos:Concept ;
    skos:broader <a623de09-bab3-4dcd-b741-f095e46660df> ;
    skos:inScheme <> ;
    skos:notation "1.3.1" ;
    skos:prefLabel "gestalten vertraute und unvertraute Gesprächssituationen verständlich, zuhörerbezogen, themenbezogen und konstruktiv,"@de .

<1d4d2be7-c91f-414e-9cb9-1aebdcf30a2e> a skos:Concept ;
    skos:broader <eb4019c2-ebcb-44cc-bff9-160b56dd3e94> ;
    skos:inScheme <> ;
    skos:notation "2.6.2" ;
    skos:prefLabel "formulieren und nutzen textsortenspezifische Kriterien für die Textüberprüfung und -überarbeitung,"@de .

<1dad27a6-afdd-4e7f-b589-fcb2789c5f4b> a skos:Concept ;
    skos:broader <a623de09-bab3-4dcd-b741-f095e46660df> ;
    skos:inScheme <> ;
    skos:notation "1.3.5" ;
    skos:prefLabel "praktizieren ein Spektrum von Gesprächs- und Lernformen, z. B. literarische Gespräche, Rollenspiele, Vorstellungsgespräche, Diskussionen, Debatten, videogestützte Gespräche, Interviews,"@de .

<1e4eef1d-6286-448e-a9e1-b4535d353662> a skos:Concept ;
    skos:broader <9a5651fb-a7d4-4d9e-9fd6-16f18c30d689> ;
    skos:inScheme <> ;
    skos:notation "2.5.4" ;
    skos:prefLabel "nutzen unterschiedliche Schreibformen: kooperative (z. B. Schreibkonferenzen), produktive (z. B. Umschreiben, Weiterschreiben) und kreative (z. B. Schreiben zu Bildern),"@de .

<214c3436-8c20-4c25-8760-4e131bdd6e17> a skos:Concept ;
    skos:broader <9cae79a8-debe-4435-82ed-1e932481fa69> ;
    skos:inScheme <> ;
    skos:notation "2.4.3" ;
    skos:prefLabel "stellen Überlegungen zu den Adressaten (unbekannt, verschiedene, abstrakt), deren unterschiedlichem Vorwissen und ihren Lesezielen an,"@de .

<229d8a7b-2009-4c26-9755-2acd0dba39ce> a skos:Concept ;
    skos:broader <0c1d1d2b-5290-4200-bc5d-3ae306b27057> ;
    skos:inScheme <> ;
    skos:notation "1.4.4" ;
    skos:prefLabel "planen und gestalten Formen des szenischen Sprechens und Spielens, z. B. szenische Lesung, Szene, Aufführung, Hörspiel, Videoclip, Film, Standbilder, Improvisation."@de .

<22edff8a-2075-4072-bd11-4fe0ac42bd09> a skos:Concept ;
    skos:broader <256b3d49-fc3b-440d-b297-33f774091001> ;
    skos:inScheme <> ;
    skos:notation "5.1.1" ;
    skos:prefLabel "Sprachliche Verständigung als Form des Handelns"@de .

<243c4565-3b98-4257-bde4-2d04f41655a7> a skos:Concept ;
    skos:broader <f6b6aa3d-8c6e-4c4c-a10b-6e87527ed962> ;
    skos:inScheme <> ;
    skos:notation "1.1.5" ;
    skos:prefLabel "bauen Redebeiträge in vertrauten und unvertrauten Situationen für andere nachvollziehbar und sinnvoll auf,"@de .

<298adf42-409e-4b4f-80f5-a013f8c12fa8> a skos:Concept ;
    skos:broader <20aab88b-20cb-412c-9a7c-3b5fbb711616> ;
    skos:inScheme <> ;
    skos:notation "4.6.3" ;
    skos:prefLabel "unterscheiden Nutzungsformen digitaler Formate und Umgebungen und stellen Zusammenhänge zwischen Intentionen, Gestaltung und Wirkung her,"@de .

<2c58911a-fedb-4972-805c-89cb9c937a91> a skos:Concept ;
    skos:broader <0c1d1d2b-5290-4200-bc5d-3ae306b27057> ;
    skos:inScheme <> ;
    skos:notation "1.4.3" ;
    skos:prefLabel "planen, organisieren und gestalten Formen des Präsentierens, z. B. Standbild, Stegreif-Rede, Referat, Podcasts, Erklärvideos,"@de .

<2cc7d262-59f4-4518-a12b-7ee23fb52188> a skos:Concept ;
    skos:broader <f3e8398a-f4b2-4329-8f0b-b2728440d89f> ;
    skos:inScheme <> ;
    skos:notation "3.3.1" ;
    skos:prefLabel "ermitteln explizite oder durch Schließen zu gewinnende Textinformationen auf lokaler Ebene und beachten dabei auch sprachliche Mittel zur Sicherung des Textzusammenhangs,"@de .

<33f4aa8c-3498-4594-9bec-1b2d6da7ed01> a skos:Concept ;
    skos:broader <9cae79a8-debe-4435-82ed-1e932481fa69> ;
    skos:inScheme <> ;
    skos:notation "2.4.8" ;
    skos:prefLabel "nutzen und reflektieren die Möglichkeiten digitaler Schreibwerkzeuge zur Vorbereitung der Textproduktion, z. B. Anfertigen von Notizen und Skizzen, Anlegen von Stoffsammlungen, Exzerpieren aus digitalen Informationsressourcen,"@de .

<355cc7fc-6b32-467c-b8c3-d382b2da61c3> a skos:Concept ;
    skos:broader <20aab88b-20cb-412c-9a7c-3b5fbb711616> ;
    skos:inScheme <> ;
    skos:notation "4.6.4" ;
    skos:prefLabel "untersuchen den Zusammenhang von digitalen Nutzerprofilen, Algorithmen, Gestaltung und Wirkung digitaler Angebote und nehmen kritisch Stellung,"@de .

<3a7fce54-94cd-492e-9ae4-e72b1b9b6fbe> a skos:Concept ;
    skos:broader <9f90ae14-f3bd-4802-9bc9-9fdc72bd1ee3> ;
    skos:inScheme <> ;
    skos:notation "5.2.4" ;
    skos:prefLabel "Aussprache"@de .

<3b91b2f8-32a4-4292-9412-33ed8262488c> a skos:Concept ;
    skos:broader <6aa6a0a9-3e2b-4f60-961e-e558121b1701> ;
    skos:inScheme <> ;
    skos:notation "4.5.3" ;
    skos:prefLabel "unterscheiden Information und Wertung in Texten,"@de .

<422c751c-6f00-4de0-adf6-4a8b398e6c7e> a skos:Concept ;
    skos:broader <20aab88b-20cb-412c-9a7c-3b5fbb711616> ;
    skos:inScheme <> ;
    skos:notation "4.6.1" ;
    skos:prefLabel "kennen medienspezifische Gestaltungsmittel und beschreiben ihre Wirkung,"@de .

<430c0587-62db-43c6-a3a7-a2092ace59c6> a skos:Concept ;
    skos:broader <eb4019c2-ebcb-44cc-bff9-160b56dd3e94> ;
    skos:inScheme <> ;
    skos:notation "2.6.4" ;
    skos:prefLabel "holen Text-Feedback ein und formulieren selbst auch Feedback zu Texten anderer, "@de .

<468453eb-8db6-4af8-b383-77181e3b288f> a skos:Concept ;
    skos:broader <9c2799d2-0508-4093-8bd0-21ba941671eb> ;
    skos:inScheme <> ;
    skos:notation "4.4.11" ;
    skos:prefLabel "gestalten Texte unterschiedlicher medialer Form szenisch,"@de .

<472c4fb3-08a4-4d43-90b4-744b825409ac> a skos:Concept ;
    skos:broader <f6b6aa3d-8c6e-4c4c-a10b-6e87527ed962> ;
    skos:inScheme <> ;
    skos:notation "1.1.8" ;
    skos:prefLabel "geben in vertrauten und unvertrauten Situationen gezielt Feedback zu Redebeiträgen,"@de .

<47a3aee8-c16f-4d93-a153-4c3d132fb271> a skos:Concept ;
    skos:broader <9c2799d2-0508-4093-8bd0-21ba941671eb> ;
    skos:inScheme <> ;
    skos:notation "4.4.7" ;
    skos:prefLabel "überprüfen eigene und fremde Deutungshypothesen,"@de .

<4b41da57-212b-442c-872c-0df9becd8bdd> a skos:Concept ;
    skos:broader <9c2799d2-0508-4093-8bd0-21ba941671eb> ;
    skos:inScheme <> ;
    skos:notation "4.4.12" ;
    skos:prefLabel "tragen literarische Texte (z. B. Gedicht, Erzählung, Szene) nach Vorbereitung sinngestaltend und ausdrucksstark vor."@de .

<4c0f9591-937a-45a1-bcd2-90684d625a23> a skos:Concept ;
    skos:broader <9c2799d2-0508-4093-8bd0-21ba941671eb> ;
    skos:inScheme <> ;
    skos:notation "4.4.8" ;
    skos:prefLabel "setzen ermittelte Aussagen und Deutungen kritisch zu eigenen Wertvorstellungen, Welt- und Selbstkonzepten in Beziehung, "@de .

<4e4fb9f1-2e0f-46fd-a4eb-692fb296b1eb> a skos:Concept ;
    skos:broader <f6b6aa3d-8c6e-4c4c-a10b-6e87527ed962> ;
    skos:inScheme <> ;
    skos:notation "1.1.9" ;
    skos:prefLabel "nutzen technische Möglichkeiten beim digitalen Kommunizieren in vertrauten und unvertrauten Situationen reflektiert."@de .

<4f176432-01b9-4a92-be8f-f71cd07c30c8> a skos:Concept ;
    skos:broader <f6b6aa3d-8c6e-4c4c-a10b-6e87527ed962> ;
    skos:inScheme <> ;
    skos:notation "1.1.6" ;
    skos:prefLabel "berücksichtigen bewusst ein Spektrum von Wirkungen ihrer Redeweise situations- und adressatengerecht: Lautstärke, Betonung, Sprechtempo, Pausen, Körpersprache (Gestik/Mimik/Körperhaltung),"@de .

<5083be0c-a654-41c4-8a61-c9877b17e147> a skos:Concept ;
    skos:broader <2345b865-7b61-49fe-aa26-2005e3855290> ;
    skos:inScheme <> ;
    skos:notation "3.1.4" ;
    skos:prefLabel "lesen in angemessenem Tempo."@de .

<577943e0-9239-4104-9c00-2f65c487ad80> a skos:Concept ;
    skos:broader <9cae79a8-debe-4435-82ed-1e932481fa69> ;
    skos:inScheme <> ;
    skos:notation "2.4.1" ;
    skos:prefLabel "sammeln Ideen, ordnen Informationen und fertigen eine Gliederung an, z. B. numerische Gliederung, Cluster, Mindmap, Flussdiagramm,"@de .

<5857b62c-b382-4da6-acd0-2b57c5a139cc> a skos:Concept ;
    skos:broader <6aa6a0a9-3e2b-4f60-961e-e558121b1701> ;
    skos:inScheme <> ;
    skos:notation "4.5.1" ;
    skos:prefLabel "erschließen aus einem oder mehreren Texten unterschiedlicher Komplexität zielgerichtet relevante Informationen, ordnen, selegieren und vergleichen diese und prüfen ihren Sachgehalt,"@de .

<5b2ac260-61fd-4722-adbd-443bfca124b1> a skos:Concept ;
    skos:broader <f3e8398a-f4b2-4329-8f0b-b2728440d89f> ;
    skos:inScheme <> ;
    skos:notation "3.3.3" ;
    skos:prefLabel "nutzen bei digitalen Texten Navigationsstrukturen (u. a. Hyperlinks) zur Gewinnung von Textinformationen,"@de .

<629cff5e-4ee2-4a35-870f-f7e831f74f27> a skos:Concept ;
    skos:broader <9a5651fb-a7d4-4d9e-9fd6-16f18c30d689> ;
    skos:inScheme <> ;
    skos:notation "2.5.8" ;
    skos:prefLabel "formulieren eigene Positionen und begründen diese"@de .

<690efb48-ccab-4ca9-93b2-f8417edbdd5c> a skos:Concept ;
    skos:broader <9c2799d2-0508-4093-8bd0-21ba941671eb> ;
    skos:inScheme <> ;
    skos:notation "4.4.10" ;
    skos:prefLabel "nutzen handlungs- und produktionsorientierte Verfahren, um ihre Deutung zu entwickeln und darzustellen und um Strukturen literarischer Texte nachzuvollziehen (z. B. innerer Monolog, Standbild, Parallelgedicht; Clip, Storyboard/ Fotostory),"@de .

<6d64491b-21fe-464b-8732-230b7f9d741b> a skos:Concept ;
    skos:broader <a623de09-bab3-4dcd-b741-f095e46660df> ;
    skos:inScheme <> ;
    skos:notation "1.3.7" ;
    skos:prefLabel "sprechen über das sprachliche und literarische Lernen sowie über Lernerfahrungen: geben Beobachtungen wieder, beschreiben Sachverhalte, geben Erklärungen und Begründungen, reflektieren."@de .

<6deccf09-f4dc-4ca7-bc9b-fe2995b6a2a4> a skos:Concept ;
    skos:broader <eec3cd54-e901-445c-b03f-f95cd3071281> ;
    skos:inScheme <> ;
    skos:notation "2.2.2" ;
    skos:prefLabel "schreiben Kernwörter richtig (Inhaltswörter und Funktionswörter)"@de .

<6e7f959a-4c36-4c10-96b6-e9fe3d507e23> a skos:Concept ;
    skos:broader <eec3cd54-e901-445c-b03f-f95cd3071281> ;
    skos:inScheme <> ;
    skos:notation "2.2.7" ;
    skos:prefLabel "nutzen Rechtschreibhilfen (Wörterbuch und digitale Rechtschreibhilfe),"@de .

<7aa8a62e-2ed8-4367-b4f5-e5601341546f> a skos:Concept ;
    skos:broader <9a5651fb-a7d4-4d9e-9fd6-16f18c30d689> ;
    skos:inScheme <> ;
    skos:notation "2.5.2" ;
    skos:prefLabel "formulieren Texte auch für unbekannte, unterschiedliche und abstrakte Adressaten angemessen, "@de .

<7aaf7ee1-2402-40a7-90df-36539d2f5e20> a skos:Concept ;
    skos:broader <9c2799d2-0508-4093-8bd0-21ba941671eb> ;
    skos:inScheme <> ;
    skos:notation "4.4.5" ;
    skos:prefLabel "stellen Bezüge zwischen literarischen Texten, Medien und deren Motiven her und nutzen Vergleiche für die Deutung und Wertung (auch intermedial, z. B. Buch und Film),"@de .

<7e01eae2-2624-46ce-8130-7e5c370d2ad7> a skos:Concept ;
    skos:broader <eec3cd54-e901-445c-b03f-f95cd3071281> ;
    skos:inScheme <> ;
    skos:notation "2.2.3" ;
    skos:prefLabel "schreiben auch wichtige Fachbegriffe und Fremdwörter richtig,"@de .

<82bbc5f0-ea5f-4d0b-9fba-24f37c4a2d14> a skos:Concept ;
    skos:broader <9a5651fb-a7d4-4d9e-9fd6-16f18c30d689> ;
    skos:inScheme <> ;
    skos:notation "2.5.1" ;
    skos:prefLabel "formulieren Texte gedanklich geordnet, verständlich, kohärenzstiftend und strukturiert, sprachlich angemessen, abwechslungsreich und differenziert,"@de .

<832e34d7-c334-4046-b132-bc5476ca90be> a skos:Concept ;
    skos:broader <268ad40b-22e9-4f11-a398-e489d91b47c4> ;
    skos:inScheme <> ;
    skos:notation "4.2.2" ;
    skos:prefLabel "bewerten Rechercheergebnisse (insbes. Internet) im Hinblick auf ihre thematische Relevanz und Verlässlichkeit (Informationsangebote vergleichen, Quellenangaben und -qualität prüfen, Informationen zu Autorinnen und Autoren recherchieren, mögliche Textfunktionen beachten)."@de .

<8383ee16-c3a6-4118-926c-4590cf7f0d52> a skos:Concept ;
    skos:broader <81f10aa7-e051-4a50-ad84-d3ff9a39017b> ;
    skos:inScheme <> ;
    skos:notation "1.2.3" ;
    skos:prefLabel "wählen aus einem Repertoire kognitiver und metakognitive Strategien aus, die sie vor, während und nach dem Zuhören einsetzen, z. B. die Erwartungshaltung zielorientiert steuern, Aufmerksamkeit steuern, Informationen sichern und zusammenfassen: Notizen, Protokoll."@de .

<85bd069f-1129-42f7-8876-ce8cc72af799> a skos:Concept ;
    skos:broader <a623de09-bab3-4dcd-b741-f095e46660df> ;
    skos:inScheme <> ;
    skos:notation "1.3.6" ;
    skos:prefLabel "wenden ein Spektrum von allgemeinen und für Gesprächs- und Lernformen spezifischen Strategien an, z. B. Gespräche steuern (Diskussionen), zielführende Rückmeldungen geben (Feedback),"@de .

<8903b15d-e18e-4fb6-bb05-1f8130d8acbc> a skos:Concept ;
    skos:broader <eb4019c2-ebcb-44cc-bff9-160b56dd3e94> ;
    skos:inScheme <> ;
    skos:notation "2.6.3" ;
    skos:prefLabel "überprüfen eigene und fremde Texte kriteriengeleitet (auch auf der Basis von Rückmeldungen) und überarbeiten diese (auch kooperativ) im Hinblick auf:"@de .

<8a608227-2aa4-42b4-9241-0cb6234c66c4> a skos:Concept ;
    skos:broader <9c2799d2-0508-4093-8bd0-21ba941671eb> ;
    skos:inScheme <> ;
    skos:notation "4.4.3" ;
    skos:prefLabel "formulieren eigene Deutungen von Texten und belegen diese,"@de .

<8adeb3be-d5ad-4e77-bb96-d461389ff14c> a skos:Concept ;
    skos:broader <20aab88b-20cb-412c-9a7c-3b5fbb711616> ;
    skos:inScheme <> ;
    skos:notation "4.6.5" ;
    skos:prefLabel "nutzen digitale Werkzeuge zielgerichtet und reflektiert für die Gestaltung eigener medialer Produkte, z. B. für Präsentationen."@de .

<8c515895-ba46-44e2-b9f4-e8e3e0d060fa> a skos:Concept ;
    skos:broader <eb4019c2-ebcb-44cc-bff9-160b56dd3e94> ;
    skos:inScheme <> ;
    skos:notation "2.6.6" ;
    skos:prefLabel "kennen sowohl allgemeine als auch textsortenspezifische Überarbeitungsstrategien und wenden diese (auch kooperativ) an. Sie reflektieren sowohl ihren Überarbeitungsprozess als auch den Strategieeinsatz."@de .

<8ca2e255-b91c-40eb-b72d-66a99c9dd2dd> a skos:Concept ;
    skos:broader <3cec36e7-400e-4804-8d6d-372991cc608e> ;
    skos:inScheme <> ;
    skos:notation "4.1.5" ;
    skos:prefLabel "kennen sprachliche Gestaltungsmittel (u. a. Wort-, Satz- und Gedankenfiguren, sprachliche Bilder, Vergleich, Metapher, Symbol) und beschreiben ihre Wirkung im Text,"@de .

<8f466de6-6770-4a86-8535-9972199d86fb> a skos:Concept ;
    skos:broader <9cae79a8-debe-4435-82ed-1e932481fa69> ;
    skos:inScheme <> ;
    skos:notation "2.4.2" ;
    skos:prefLabel "sammeln vielfältige sprachliche und gestalterische Mittel: Wörter und Wortfelder, Formulierungen,"@de .

<917281b0-bf0c-4ca9-97e9-54659d78423f> a skos:Concept ;
    skos:broader <9c2799d2-0508-4093-8bd0-21ba941671eb> ;
    skos:inScheme <> ;
    skos:notation "4.4.1" ;
    skos:prefLabel "nutzen implizite und explizite Informationen zu Figuren und Figurenkonstellationen, zu Raum- und Zeitdarstellung, Handlungs- und Konfliktverlauf sowie Atmosphäre zum Aufbau von Textverständnis und zur Entwicklung einer differenzierten Deutung,"@de .

<9226fd24-3371-4d1a-a3a6-f804ba85bb42> a skos:Concept ;
    skos:broader <eec3cd54-e901-445c-b03f-f95cd3071281> ;
    skos:inScheme <> ;
    skos:notation "2.2.1" ;
    skos:prefLabel "kennen wort- und satzbezogene Rechtschreibstrategien und wenden diese sicher an"@de .

<92db1ed8-e8a4-41a9-8ece-8d59dc1c77e8> a skos:Concept ;
    skos:broader <0c1d1d2b-5290-4200-bc5d-3ae306b27057> ;
    skos:inScheme <> ;
    skos:notation "1.4.1" ;
    skos:prefLabel "leisten längere, auch freie Beiträge adressatenorientiert und mit Hilfe eines Stichwortzettels/einer Gliederung/von Notizen/eines Textes, z. B. Referat, Präsentation, Rede, Gedicht,"@de .

<93c56417-f2ab-4178-a65a-cfc4873faad9> a skos:Concept ;
    skos:broader <eec3cd54-e901-445c-b03f-f95cd3071281> ;
    skos:inScheme <> ;
    skos:notation "2.2.8" ;
    skos:prefLabel "überprüfen und korrigieren Texte auf orthografische Richtigkeit."@de .

<943da7cf-bb2b-4365-9e4f-59a0793ca1b6> a skos:Concept ;
    skos:broader <5ac9209d-24d0-4460-914f-83129752f00e> ;
    skos:inScheme <> ;
    skos:notation "3.4.1" ;
    skos:prefLabel "bereiten das Lesen vor (u. a. Leseziel formulieren, Vorwissen aktivieren, Erwartungen an den Text formulieren, nach Möglichkeit die Textsorte klären),"@de .

<95ed6c76-f533-4087-b5dc-b05d06db239a> a skos:Concept ;
    skos:broader <9cae79a8-debe-4435-82ed-1e932481fa69> ;
    skos:inScheme <> ;
    skos:notation "2.4.5" ;
    skos:prefLabel "entwickeln gemäß der Schreibaufgabe und der Zeitvorgabe einen Schreibplan, entscheiden sich für die angemessene Textsorte und konzipieren einen ziel-, adressaten- und situationsbezogenen Text,"@de .

<991930f2-5734-4526-9627-0274e1a32b2d> a skos:Concept ;
    skos:broader <f3e8398a-f4b2-4329-8f0b-b2728440d89f> ;
    skos:inScheme <> ;
    skos:notation "3.3.2" ;
    skos:prefLabel "stellen bei multimodalen Texten Bezüge zwischen Text, Bild und Ton her,"@de .

<9989937a-b304-4bfe-87b2-8a0332e6643c> a skos:Concept ;
    skos:broader <2345b865-7b61-49fe-aa26-2005e3855290> ;
    skos:inScheme <> ;
    skos:notation "3.1.2" ;
    skos:prefLabel "verfügen über eine sichere Worterkennung (bei selteneren Wörtern mit Selbstkorrektur),"@de .

<9acef28a-e507-4451-a1e1-b5933ce66aa3> a skos:Concept ;
    skos:broader <9f90ae14-f3bd-4802-9bc9-9fdc72bd1ee3> ;
    skos:inScheme <> ;
    skos:notation "5.2.3" ;
    skos:prefLabel "Rechtschreibung"@de .

<9bcfcdc8-e37b-4a06-97c5-12cfeaa9cc45> a skos:Concept ;
    skos:broader <9c2799d2-0508-4093-8bd0-21ba941671eb> ;
    skos:inScheme <> ;
    skos:notation "4.4.9" ;
    skos:prefLabel "tauschen sich mit anderen zu Text- bzw. Medienwahrnehmungen, -deutungen und -wertungen aus, setzen sich mit vorgebrachten Deutungen und Argumenten auseinander und formulieren fundierte eigene Auffassungen (u. a. literarisches Gespräch, Auswertung szenischer Gestaltung),"@de .

<9d35ab93-1152-48c8-ae6a-b737bc743db5> a skos:Concept ;
    skos:broader <9a5651fb-a7d4-4d9e-9fd6-16f18c30d689> ;
    skos:inScheme <> ;
    skos:notation "2.5.3" ;
    skos:prefLabel "realisieren sicher mit einem Spektrum unterschiedlicher Textsorten grundlegende kommunikative und personal-heuristische Schreibfunktionen:"@de .

<a43dddb1-9337-4451-99de-49e53915f404> a skos:Concept ;
    skos:broader <9c2799d2-0508-4093-8bd0-21ba941671eb> ;
    skos:inScheme <> ;
    skos:notation "4.4.4" ;
    skos:prefLabel "formulieren Wertungen von Texten und begründen sie differenziert,"@de .

<a6a55f4e-402e-4d97-835e-1a63fdcbabc7> a skos:Concept ;
    skos:broader <f3e8398a-f4b2-4329-8f0b-b2728440d89f> ;
    skos:inScheme <> ;
    skos:notation "3.3.4" ;
    skos:prefLabel "verknüpfen Textinformationen, ziehen auch unter Nutzung ihres Vorwissens Schlussfolgerungen und konstruieren ein Gesamtverständnis."@de .

<a7503296-8c8d-40dd-8a0a-2f31e228d407> a skos:Concept ;
    skos:broader <9cae79a8-debe-4435-82ed-1e932481fa69> ;
    skos:inScheme <> ;
    skos:notation "2.4.6" ;
    skos:prefLabel "nutzen Informationsquellen gezielt, angemessen und reflektiert, insbesondere Bibliotheken, Nachschlagewerke, Zeitungen, Newsportale, digitale Suchdienste und (digitale) Ressourcen zur deutschen Sprache,"@de .

<a7c50eb3-ed85-4b58-8d50-3a3e8b6cd990> a skos:Concept ;
    skos:broader <3cec36e7-400e-4804-8d6d-372991cc608e> ;
    skos:inScheme <> ;
    skos:notation "4.1.10" ;
    skos:prefLabel "kennen typische Formen digitaler Textstrukturierung (u. a. Hypertext, Hyperlinks, multimodale Textgestaltung) und nutzen sie zielorientiert (u. a. bei Recherchen und Präsentationen)."@de .

<a806abff-5a41-46e2-ba1f-ba742b3a77af> a skos:Concept ;
    skos:broader <9f90ae14-f3bd-4802-9bc9-9fdc72bd1ee3> ;
    skos:inScheme <> ;
    skos:notation "5.2.5" ;
    skos:prefLabel "Ressourcen zur deutschen Sprache"@de .

<ac670c29-a934-440f-8bef-5742e04d254e> a skos:Concept ;
    skos:broader <6aa6a0a9-3e2b-4f60-961e-e558121b1701> ;
    skos:inScheme <> ;
    skos:notation "4.5.4" ;
    skos:prefLabel "unterscheiden informierende, erzählende und appellierende Elemente in Texten,"@de .

<adc64464-7367-42d2-933a-430592d1f104> a skos:Concept ;
    skos:broader <20aab88b-20cb-412c-9a7c-3b5fbb711616> ;
    skos:inScheme <> ;
    skos:notation "4.6.2" ;
    skos:prefLabel "unterscheiden digitale Angebote in Bezug auf ihre Funktion und Wirkung (z. B. informierend, unterhaltend, appellierend),"@de .

<af55c267-cd04-46bd-a64a-25b4248d77e0> a skos:Concept ;
    skos:broader <81f10aa7-e051-4a50-ad84-d3ff9a39017b> ;
    skos:inScheme <> ;
    skos:notation "1.2.1" ;
    skos:prefLabel "können in einem Spektrum vertrauter und unvertrauter Kommunikationssituationen"@de .

<b0925c9d-c22f-4c80-9e93-63c4708bcb50> a skos:Concept ;
    skos:broader <3cec36e7-400e-4804-8d6d-372991cc608e> ;
    skos:inScheme <> ;
    skos:notation "4.1.9" ;
    skos:prefLabel "nutzen ein Spektrum auditiver und audiovisueller Medien für persönliche oder schulische Zwecke bedarfsgerecht (z. B. Hörspiel, Lesung, Hörbuch, Podcast, Wissenssendung, Erklärvideo, Tutorial, Webinar),"@de .

<b3702a19-bd38-466a-a940-b843f80d9d84> a skos:Concept ;
    skos:broader <9cae79a8-debe-4435-82ed-1e932481fa69> ;
    skos:inScheme <> ;
    skos:notation "2.4.9" ;
    skos:prefLabel "wenden Planungsstrategien (auch kooperativ) an und reflektieren sowohl ihren Planungsprozess als auch den Strategieeinsatz."@de .

<b5d718a7-9fd5-46c8-94a1-60977a5d3d1e> a skos:Concept ;
    skos:broader <9cae79a8-debe-4435-82ed-1e932481fa69> ;
    skos:inScheme <> ;
    skos:notation "2.4.7" ;
    skos:prefLabel "planen Texte kooperativ und kollaborativ und strukturieren diese: z. B. Textbaupläne,"@de .

<b66ac4e0-a5e8-4d7c-b9a4-a2e2a80daf7a> a skos:Concept ;
    skos:broader <eb4019c2-ebcb-44cc-bff9-160b56dd3e94> ;
    skos:inScheme <> ;
    skos:notation "2.6.1" ;
    skos:prefLabel "erkennen den Überarbeitungsbedarf von Texten, überlegen Schritte zur Textüberarbeitung und setzen diese um,"@de .

<b8bc6d6b-96b3-4aca-b1a2-592978c889ee> a skos:Concept ;
    skos:broader <3cec36e7-400e-4804-8d6d-372991cc608e> ;
    skos:inScheme <> ;
    skos:notation "4.1.3" ;
    skos:prefLabel "kennen Merkmale medialer und szenischer Gestaltung (u. a. Musik, Geräusche, Farben, Perspektive, Animation, Bühnenbild) und ihre Wirkung,"@de .

<bfeff818-15a8-4d5d-b04e-fe9431c70007> a skos:Concept ;
    skos:broader <fa750b0d-951a-4bb0-b770-56d53059170e> ;
    skos:inScheme <> ;
    skos:notation "2.1.2" ;
    skos:prefLabel "schreiben Texte (handschriftlich leserlich und mithilfe digitaler Schreibwerkzeuge) in einem der Situation angemessenen Tempo, z. B. persönliche Notiz vs. handschriftlicher Brief,"@de .

<c4a23361-0904-4ea2-a49f-25d0981ab7e2> a skos:Concept ;
    skos:broader <a623de09-bab3-4dcd-b741-f095e46660df> ;
    skos:inScheme <> ;
    skos:notation "1.3.3" ;
    skos:prefLabel "formulieren in vertrauten und unvertrauten Situationen angemessen; gehen auf die Gesprächspartnerin bzw. den Gesprächspartner und die jeweilige Situation ein,"@de .

<c599e0c2-d40d-45a8-b2e2-06a682eea41a> a skos:Concept ;
    skos:broader <eec3cd54-e901-445c-b03f-f95cd3071281> ;
    skos:inScheme <> ;
    skos:notation "2.2.6" ;
    skos:prefLabel "verfügen über Fehleraufmerksamkeit und nutzen Strategien zur Klärung orthografischer Fragen,"@de .

<ca49a0a9-d3a0-4af1-b2b0-38aa31b51e11> a skos:Concept ;
    skos:broader <3cec36e7-400e-4804-8d6d-372991cc608e> ;
    skos:inScheme <> ;
    skos:notation "4.1.4" ;
    skos:prefLabel "stellen an ausgewählten Beispielen Zusammenhänge zwischen Text und Kontext her (insbes. Entstehungszeit, Epoche, Leben des Autors/der Autorin),"@de .

<ca5777e5-c320-4e66-a5be-24ac931f8a8e> a skos:Concept ;
    skos:broader <9cae79a8-debe-4435-82ed-1e932481fa69> ;
    skos:inScheme <> ;
    skos:notation "2.4.4" ;
    skos:prefLabel "klären entsprechend dem Schreibziel die Schreibabsicht, Schreibsituation und Textfunktion,"@de .

<cbdcac1c-146f-4780-94e4-1434d1f9d03d> a skos:Concept ;
    skos:broader <9a5651fb-a7d4-4d9e-9fd6-16f18c30d689> ;
    skos:inScheme <> ;
    skos:notation "2.5.9" ;
    skos:prefLabel "integrieren Textbelege und andere Quellen formal richtig in den eigenen Text (zitieren, paraphrasieren),"@de .

<cca7c021-b4b6-417b-8411-46194a03702f> a skos:Concept ;
    skos:broader <6aa6a0a9-3e2b-4f60-961e-e558121b1701> ;
    skos:inScheme <> ;
    skos:notation "4.5.6" ;
    skos:prefLabel "beziehen Textaussagen auf eigene Welt- und Wertvorstellungen und überprüfen diese,"@de .

<d29708e8-0633-4f46-9ed4-5adafb7552e2> a skos:Concept ;
    skos:broader <f6b6aa3d-8c6e-4c4c-a10b-6e87527ed962> ;
    skos:inScheme <> ;
    skos:notation "1.1.3" ;
    skos:prefLabel "kommunizieren mit Blick auf Adressaten und Ziele situationsangemessen:"@de .

<d3150b72-c776-4f9b-ab10-2c5726e6c871> a skos:Concept ;
    skos:broader <f6b6aa3d-8c6e-4c4c-a10b-6e87527ed962> ;
    skos:inScheme <> ;
    skos:notation "1.1.2" ;
    skos:prefLabel "verfügen über einen angemessenen Wortschatz,"@de .

<d4aff95b-4f46-4ab6-9f56-b55fd0d079c6> a skos:Concept ;
    skos:broader <fa750b0d-951a-4bb0-b770-56d53059170e> ;
    skos:inScheme <> ;
    skos:notation "2.1.3" ;
    skos:prefLabel "gestalten Texte (handschriftlich und mithilfe digitaler Schreibwerkzeuge) übersichtlich und zielorientiert, bauen sie sinnvoll auf und strukturieren diese: z. B. Blattaufteilung, Seitenränder, Absatzstrukturen, Textformatierung und Layout."@de .

<d5bc7360-7434-4a5c-b64b-4933c70cbb42> a skos:Concept ;
    skos:broader <2345b865-7b61-49fe-aa26-2005e3855290> ;
    skos:inScheme <> ;
    skos:notation "3.1.1" ;
    skos:prefLabel "nutzen die schriftsprachlichen Informationen (u. a. Silbe, Morphem, Komma, Punkt, Fragezeichen, Großschreibung) beim Lesen,"@de .

<d75230b0-c74f-4456-b56b-a9efc13469e2> a skos:Concept ;
    skos:broader <3afda5c9-fdc3-48a5-9f69-848b725afc7f> ;
    skos:inScheme <> ;
    skos:notation "3.2.1" ;
    skos:prefLabel "ermitteln explizite oder durch Schließen zu gewinnende Textinformationen auf lokaler Ebene und beachten dabei auch sprachliche Mittel zur Sicherung des Textzusammenhangs,"@de .

<dd78d412-9590-4da5-b86a-09b65c2972ef> a skos:Concept ;
    skos:broader <268ad40b-22e9-4f11-a398-e489d91b47c4> ;
    skos:inScheme <> ;
    skos:notation "4.2.1" ;
    skos:prefLabel "nutzen unterschiedliche Informationsangebote und Recherchemöglichkeiten (z. B. Kritiken, Bestenlisten und andere Empfehlungen; Internet, Zeitschriften, Zeitungen, Hörfunk, Fernsehen, Bücherei),"@de .

<dfceddcb-cfaf-47bb-a377-0a54c0a4331b> a skos:Concept ;
    skos:broader <0c1d1d2b-5290-4200-bc5d-3ae306b27057> ;
    skos:inScheme <> ;
    skos:notation "1.4.2" ;
    skos:prefLabel "nutzen typische Werkzeuge für das Sprechen vor anderen, z. B. Tafel, Plakat, Moderationskarten, Präsentationssoftware, Whiteboard,"@de .

<e1ac6c8e-61e7-4046-b976-33d935a8738e> a skos:Concept ;
    skos:broader <3cec36e7-400e-4804-8d6d-372991cc608e> ;
    skos:inScheme <> ;
    skos:notation "4.1.6" ;
    skos:prefLabel "unterscheiden zentrale Textmuster (Erzählen, Berichten, Beschreiben, Argumentieren),"@de .

<e21ce5dd-cd67-465e-ac3e-e43a690fabb0> a skos:Concept ;
    skos:broader <6aa6a0a9-3e2b-4f60-961e-e558121b1701> ;
    skos:inScheme <> ;
    skos:notation "4.5.2" ;
    skos:prefLabel "werten auch komplexere nichtlineare und multimodale Texte (Text-Bild-Bezüge) zielorientiert aus, z. B. um über ein Thema zu sprechen oder zu schreiben,"@de .

<e305aceb-6c61-4008-9032-5ff947e562f3> a skos:Concept ;
    skos:broader <9a5651fb-a7d4-4d9e-9fd6-16f18c30d689> ;
    skos:inScheme <> ;
    skos:notation "2.5.5" ;
    skos:prefLabel "kennen und nutzen die Möglichkeiten digitaler Schreibwerkzeuge zur Formulierung und (formalen) Gestaltung von Texten, z. B. Schrift‐ und Textformatierung, Arrangieren und Re‐Arrangieren von Textbausteinen und Medienobjekten,"@de .

<e54ea95f-ec06-4686-9a55-627bdcf808a6> a skos:Concept ;
    skos:broader <9a5651fb-a7d4-4d9e-9fd6-16f18c30d689> ;
    skos:inScheme <> ;
    skos:notation "2.5.10" ;
    skos:prefLabel "kennen Formulierungsstrategien und wenden diese (auch kooperativ) an. Sie reflektieren ihren Strategieeinsatz."@de .

<e594e1b7-7bc8-4cb5-8871-094215709322> a skos:Concept ;
    skos:broader <eec3cd54-e901-445c-b03f-f95cd3071281> ;
    skos:inScheme <> ;
    skos:notation "2.2.4" ;
    skos:prefLabel "schreiben Satzanfänge, Eigennamen und Kerne von Nominalgruppen groß,"@de .

<e905e089-c20e-4928-adc6-9cb8643974de> a skos:Concept ;
    skos:broader <9c2799d2-0508-4093-8bd0-21ba941671eb> ;
    skos:inScheme <> ;
    skos:notation "4.4.2" ;
    skos:prefLabel "nutzen Fachbegriffe bei der Erschließung (u. a. Autor, Erzähler, Erzählperspektive, Erzählzeit/erzählte Zeit, Figur, Monolog, Dialog, Szene, Regieanweisung; Reim, Vers, Sprecher/lyrischer Sprecher; Kameraperspektive, Einstellungsgröße, Montage, Kamerabewegung, Schnitt),"@de .

<e9e07412-593c-4ee6-86ab-71f623d2f149> a skos:Concept ;
    skos:broader <5ac9209d-24d0-4460-914f-83129752f00e> ;
    skos:inScheme <> ;
    skos:notation "3.4.2" ;
    skos:prefLabel "lesen Texte organisierend (u. a. für das Leseziel wichtige Aussagen markieren, unbekannte Wörter unterstreichen und nachschlagen), elaborierend (u. a. Annahmen zur Textaussage formulieren, Fragen an den Text stellen, über den Text hinaus denken, assoziieren) und reflektierend (das Textverstehen prüfen, Verstehensschwierigkeiten erkennen und beheben sowie den Strategieeinsatz selbst reflektieren),"@de .

<ea58a52f-e8da-47df-9674-df5a41970595> a skos:Concept ;
    skos:broader <9f90ae14-f3bd-4802-9bc9-9fdc72bd1ee3> ;
    skos:inScheme <> ;
    skos:notation "5.2.2" ;
    skos:prefLabel "Texte und Gespräche"@de .

<ebd24922-ae93-4856-a233-9327fc4b2499> a skos:Concept ;
    skos:broader <f6b6aa3d-8c6e-4c4c-a10b-6e87527ed962> ;
    skos:inScheme <> ;
    skos:notation "1.1.7" ;
    skos:prefLabel "wählen geeignete (multimodale) Medien sinnvoll aus und setzen diese gezielt ein, um den Inhalt und die Wirkung eigener Sprechbeiträge zu unterstützen,"@de .

<edc5e5d2-fe60-4ee5-9d3b-20d61dd2b4b4> a skos:Concept ;
    skos:broader <3cec36e7-400e-4804-8d6d-372991cc608e> ;
    skos:inScheme <> ;
    skos:notation "4.1.2" ;
    skos:prefLabel "unterscheiden epische, lyrische und dramatische Texte, insbesondere epische Kleinformen (z. B. Kurzgeschichte, Fabel), Erzählung, Novelle, Roman, Jugendroman, Gedichte, Drama (z. B. Komödie, Tragödie),"@de .

<ee3206b0-4566-4ecc-881f-a8116c399804> a skos:Concept ;
    skos:broader <81f10aa7-e051-4a50-ad84-d3ff9a39017b> ;
    skos:inScheme <> ;
    skos:notation "1.2.2" ;
    skos:prefLabel "hören in einem Spektrum vertrauter und unvertrauter Situationen sinnverstehend zu,"@de .

<f3e50df3-75a3-462b-91d1-80c570aec2b5> a skos:Concept ;
    skos:broader <eb4019c2-ebcb-44cc-bff9-160b56dd3e94> ;
    skos:inScheme <> ;
    skos:notation "2.6.5" ;
    skos:prefLabel "nutzen bei der Textüberarbeitung digitale Schreibwerkzeuge (auch kooperativ), z. B. Umstellen und Modifizieren von Textbausteinen und Medienobjekten, Nachverfolgen von Änderungen, Kommentarfunktion, digitale Korrekturhilfen, und reflektieren diese,"@de .

<f6a13f0f-15b2-4077-ae9b-ac19ff8dfdfb> a skos:Concept ;
    skos:broader <9f90ae14-f3bd-4802-9bc9-9fdc72bd1ee3> ;
    skos:inScheme <> ;
    skos:notation "5.2.1" ;
    skos:prefLabel "Wörter und Sätze"@de .

<f9070ac0-ed3f-4b91-acbe-a3fa3dc858c8> a skos:Concept ;
    skos:broader <5ac9209d-24d0-4460-914f-83129752f00e> ;
    skos:inScheme <> ;
    skos:notation "3.4.3" ;
    skos:prefLabel "bereiten das Lesen nach (u. a. zentrale Textaussagen wiedergeben, wertende Einschätzung zum Text abgeben, anfängliche Erwartungen und Annahmen zur Textaussage prüfen und ggf. korrigieren, eigenen Wissenszuwachs prüfen)."@de .

<f9f5594e-5298-4401-9b35-e1030d66edef> a skos:Concept ;
    skos:broader <9c2799d2-0508-4093-8bd0-21ba941671eb> ;
    skos:inScheme <> ;
    skos:notation "4.4.6" ;
    skos:prefLabel "vergleichen Handlungen und Handlungsmotive von Figuren mit vertrauten Handlungen und Handlungsmotiven und nehmen begründete Wertungen vor,"@de .

<fdcfd575-94c8-436d-9570-6031a7088cc5> a skos:Concept ;
    skos:broader <fa750b0d-951a-4bb0-b770-56d53059170e> ;
    skos:inScheme <> ;
    skos:notation "2.1.1" ;
    skos:prefLabel "schreiben Wörter, Wortgruppen und Sätze flüssig, d. h. schnell, sicher und korrekt (automatisiert), und formulieren ihre Gedanken flüssig,"@de .

<fe2aaad2-b466-48c6-b9fb-48fc3afb6d51> a skos:Concept ;
    skos:broader <3cec36e7-400e-4804-8d6d-372991cc608e> ;
    skos:inScheme <> ;
    skos:notation "4.1.1" ;
    skos:prefLabel "kennen ein Spektrum von Werken bedeutender Autorinnen und Autoren und setzen sich mit den Texten und ihren Leseerfahrungen auseinander,"@de .

<ff1dcf39-f9a7-453b-a31b-af506504f6e8> a skos:Concept ;
    skos:broader <2345b865-7b61-49fe-aa26-2005e3855290> ;
    skos:inScheme <> ;
    skos:notation "3.1.3" ;
    skos:prefLabel "lesen komplexere Texte sinngestaltend vor,"@de .

<ff4833be-31fb-461d-929f-ec8db79210b2> a skos:Concept ;
    skos:broader <256b3d49-fc3b-440d-b297-33f774091001> ;
    skos:inScheme <> ;
    skos:notation "5.1.3" ;
    skos:prefLabel "Ausprägungen und Bedingungen sprachlicher Variation und Vielfalt"@de .

<3afda5c9-fdc3-48a5-9f69-848b725afc7f> a skos:Concept ;
    skos:broader <905ede99-a684-4947-9274-c99d143b569e> ;
    skos:definition "Die Schülerinnen und Schüler verfügen über Techniken zur Orientierung in und zwischen Texten."@de ;
    skos:inScheme <> ;
    skos:narrower <d75230b0-c74f-4456-b56b-a9efc13469e2> ;
    skos:notation "3.2" ;
    skos:prefLabel "Lesefertigkeiten: Über Lesetechniken verfügen"@de .

<268ad40b-22e9-4f11-a398-e489d91b47c4> a skos:Concept ;
    skos:broader <d72f4980-ad03-45e9-a022-1db9c07af1ec> ;
    skos:definition "Die Schülerinnen und Schüler nutzen das Medienangebot zur Entwicklung und Pflege eigener Interessen, zur Erweiterung ihrer Wissensbasis und Urteilsfähigkeit und als Anlass zum Austausch mit anderen."@de ;
    skos:inScheme <> ;
    skos:narrower <832e34d7-c334-4046-b132-bc5476ca90be>,
        <dd78d412-9590-4da5-b86a-09b65c2972ef> ;
    skos:notation "4.2" ;
    skos:prefLabel "Sich im Medienangebot orientieren"@de .

<256b3d49-fc3b-440d-b297-33f774091001> a skos:Concept ;
    skos:broader <4ac119b4-0851-40e7-ad91-3a8276da8ac0> ;
    skos:definition "Die Schülerinnen und Schüler untersuchen und reflektieren sprachliche Verständigung und sprachliche Variation in Texten, Gesprächen und Formen digitaler Kommunikation und Unterschiede zwischen Mündlichkeit und Schriftlichkeit. Sie nutzen ihre Einsichten für das eigene Sprachhandeln."@de ;
    skos:inScheme <> ;
    skos:narrower <0afa8cc3-5ca0-4815-bc2d-50524e9c706f>,
        <22edff8a-2075-4072-bd11-4fe0ac42bd09>,
        <ff4833be-31fb-461d-929f-ec8db79210b2> ;
    skos:notation "5.1" ;
    skos:prefLabel "Sprachliche Verständigung und sprachliche Vielfalt untersuchen "@de .

<4ac119b4-0851-40e7-ad91-3a8276da8ac0> a skos:Concept ;
    skos:broader <> ;
    skos:definition "Sprache und Sprachgebrauch bilden den zentralen Gegenstand des Deutschunterrichts. Die Schülerinnen und Schüler sind sich der Leistungen von Sprache als Kommunikationsmedium sowie der Bedingungen ihrer situations-, adressaten- und intentionsangemessenen Verwendung bewusst. Sie untersuchen das komplexe Erscheinungsbild sprachlichen Handelns – des eigenen und fremden – sowie die Bedingungen, unter denen es zustande kommt. Sie verfügen über erweiterte Kenntnisse des Sprachsystems. Die erworbenen Einsichten tragen zur Vertiefung ihrer Sprachbewusstheit und zur eigenen Sprachentwicklung bei. Im Sinne von „Sprache im Gebrauch“ setzen sie sich dabei mit dem umfassenden Bereich menschlicher Verständigung, sprachlicher Variation und Mehrsprachigkeit auseinander. Sie untersuchen Texte, Gespräche und Formen digitaler Kommunikation unter funktionalen Aspekten und in Hinblick auf die Angemessenheit ihrer sprachlichen Gestaltung. Im Sinne von „Sprache als System“ nehmen sie strukturelle Erscheinungen (Sätze, Wörter) wie auch deren Leistungen für die Konstitution von Texten und Gesprächen in den Blick und nutzen diese für die Produktion und Überarbeitung eigener Texte sowie für die Planung und Gestaltung von Gesprächsbeiträgen und Präsentationen. Sie nutzen Fachbegriffe nicht im Sinne eines isolierten Vokabelwissens, sondern stets im funktionalen Zusammenhang als Instrumente für die systematische Beschreibung sprachlicher Formen und ihrer Funktionen sowie für die Analyse von Texten, Gesprächen und Formen digitaler Kommunikation. Für die Entwicklung orthografischen Wissens, das für das Lesen und Schreiben grundlegend ist, liefert die Reflexion über Sprache relevante sprachsystematische Grundlagen; für das Sprechen und Zuhören liefert sie die Kenntnis der Konventionen der gesprochenen Standardsprache, für die Auseinandersetzung mit Texten und Medien wichtige Kategorien der Sprachbetrachtung. Die Schülerinnen und Schüler können ihr Wissen über Sprache im eigenen Sprachhandeln nutzen: In den mündlichen Äußerungen beachten die Schülerinnen und Schüler wichtige Regeln der Aussprache, in den schriftlichen die der Orthografie und Zeichensetzung. Sie wählen das jeweils angemessene sprachliche Register. Der Aufbau von Kompetenzen für den reflektierten Umgang mit digitalen Ressourcen zur deutschen Sprache bildet eine wichtige Grundlage für die Weiterentwicklung der Sprachbewusstheit, für die Produktion, Erschließung und Analyse von Texten und Medien und für die Planung mündlicher Präsentationen."@de ;
    skos:inScheme <> ;
    skos:narrower <256b3d49-fc3b-440d-b297-33f774091001>,
        <9f90ae14-f3bd-4802-9bc9-9fdc72bd1ee3> ;
    skos:notation "5" ;
    skos:prefLabel "Sprache und Sprachgebrauch untersuchen"@de ;
    skos:topConceptOf <> .

<5ac9209d-24d0-4460-914f-83129752f00e> a skos:Concept ;
    skos:broader <905ede99-a684-4947-9274-c99d143b569e> ;
    skos:definition "Die Schülerinnen und Schüler verfügen über ein Repertoire verschiedener kognitiver und metakognitiver Lesestrategien zum Einsatz vor dem Lesen, während des Lesens und nach dem Lesen. Sie können die Strategien beim Lesen eines unbekannten oder schwierigen Textes oder beim vergleichenden Lesen mehrerer Texte den Anforderungen der Texte und dem Leseziel entsprechend auswählen und ausführen. Sie reflektieren sowohl ihr Verstehen als auch ihren Einsatz von Lesestrategien."@de ;
    skos:inScheme <> ;
    skos:narrower <943da7cf-bb2b-4365-9e4f-59a0793ca1b6>,
        <e9e07412-593c-4ee6-86ab-71f623d2f149>,
        <f9070ac0-ed3f-4b91-acbe-a3fa3dc858c8> ;
    skos:notation "3.4" ;
    skos:prefLabel "Lesefähigkeiten: Über Strategien zum Leseverstehen verfügen"@de .

<81f10aa7-e051-4a50-ad84-d3ff9a39017b> a skos:Concept ;
    skos:broader <aa550638-26ee-4212-81af-59b54d4351d9> ;
    skos:definition "Die Schülerinnen und Schüler können die Aufmerksamkeit auf die Beiträge anderer richten und ihr Verstehen in einem Spektrum vertrauter und unvertrauter Situationen zum Ausdruck bringen."@de ;
    skos:inScheme <> ;
    skos:narrower <8383ee16-c3a6-4118-926c-4590cf7f0d52>,
        <af55c267-cd04-46bd-a64a-25b4248d77e0>,
        <ee3206b0-4566-4ecc-881f-a8116c399804> ;
    skos:notation "1.2" ;
    skos:prefLabel "Verstehend zuhören"@de .

<fa750b0d-951a-4bb0-b770-56d53059170e> a skos:Concept ;
    skos:broader <fdb78af2-b1fb-4554-a1ff-b88c9ae8a7af> ;
    skos:definition "Die Schülerinnen und Schüler schreiben flüssig; eigene Gedanken formulieren sie flüssig. Sie schreiben in einer leserlichen Handschrift und mithilfe digitaler Schreibwerkzeuge in einem der Situation angemessenen Tempo und gestalten ihre Texte zweckmäßig und übersichtlich sowie sinnvoll strukturiert."@de ;
    skos:inScheme <> ;
    skos:narrower <bfeff818-15a8-4d5d-b04e-fe9431c70007>,
        <d4aff95b-4f46-4ab6-9f56-b55fd0d079c6>,
        <fdcfd575-94c8-436d-9570-6031a7088cc5> ;
    skos:notation "2.1" ;
    skos:prefLabel "Über Schreibfertigkeiten verfügen"@de .

<0c1d1d2b-5290-4200-bc5d-3ae306b27057> a skos:Concept ;
    skos:broader <aa550638-26ee-4212-81af-59b54d4351d9> ;
    skos:definition "Die Schülerinnen und Schüler können über einen längeren Zeitraum (vorbereitet und durch Medien gestützt) sprechen und eigene kommunikative Ziele verfolgen (Ausdrucks-, Darstellungs- und Appellfunktion)."@de ;
    skos:inScheme <> ;
    skos:narrower <229d8a7b-2009-4c26-9755-2acd0dba39ce>,
        <2c58911a-fedb-4972-805c-89cb9c937a91>,
        <92db1ed8-e8a4-41a9-8ece-8d59dc1c77e8>,
        <dfceddcb-cfaf-47bb-a377-0a54c0a4331b> ;
    skos:notation "1.4" ;
    skos:prefLabel "Vor anderen sprechen"@de .

<2345b865-7b61-49fe-aa26-2005e3855290> a skos:Concept ;
    skos:broader <905ede99-a684-4947-9274-c99d143b569e> ;
    skos:definition "Die Schülerinnen und Schüler lesen leise und laut, automatisiert, genau, sinngestaltend und zügig."@de ;
    skos:inScheme <> ;
    skos:narrower <5083be0c-a654-41c4-8a61-c9877b17e147>,
        <9989937a-b304-4bfe-87b2-8a0332e6643c>,
        <d5bc7360-7434-4a5c-b64b-4933c70cbb42>,
        <ff1dcf39-f9a7-453b-a31b-af506504f6e8> ;
    skos:notation "3.1" ;
    skos:prefLabel "Lesefertigkeit: Flüssig lesen"@de .

<f3e8398a-f4b2-4329-8f0b-b2728440d89f> a skos:Concept ;
    skos:broader <905ede99-a684-4947-9274-c99d143b569e> ;
    skos:definition "Die Schülerinnen und Schüler lesen Texte mittlerer Komplexität sinnverstehend."@de ;
    skos:inScheme <> ;
    skos:narrower <2cc7d262-59f4-4518-a12b-7ee23fb52188>,
        <5b2ac260-61fd-4722-adbd-443bfca124b1>,
