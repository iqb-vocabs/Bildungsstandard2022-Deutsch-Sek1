@base <https://w3id.org/iqb/bista22des1/stdMSA/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<00dd2ff5-ef89-4a56-863c-4595fd1a83d2> a skos:Concept ;
    skos:broader <9f1e3971-4bff-4941-9606-d56b951c835b> ;
    skos:definition "Die Schülerinnen und Schüler erschließen sich ein Spektrum von Texten unterschiedlicher medialer Form und Komplexität – auch als Grundlage für die mündliche und schriftliche Anschlusskommunikation."@de ;
    skos:inScheme <> ;
    skos:notation "4.3" ;
    skos:prefLabel "Texte in unterschiedlicher medialer Form erschließen und nutzen"@de .

<028bf700-e934-4ac3-b6a0-ab2cbf675ef7> a skos:Concept ;
    skos:broader <7397461d-c0d2-4350-aa6f-75bdb9c12752> ;
    skos:inScheme <> ;
    skos:notation "1.1.5" ;
    skos:prefLabel "bauen Redebeiträge in vertrauten und unvertrauten Situationen für andere nachvollziehbar und sinnvoll auf,"@de .

<044d61cc-11d3-496d-894c-e5e423a4a362> a skos:Concept ;
    skos:broader <b0e6363b-14e6-40a7-9d71-28705c9fb358> ;
    skos:inScheme <> ;
    skos:notation "2.1.1" ;
    skos:prefLabel "schreiben Wörter, Wortgruppen und Sätze flüssig, d. h. schnell, sicher und korrekt (automatisiert), und formulieren ihre Gedanken flüssig,"@de .

<050bca0c-4cab-45d1-addd-6a8ffcceb57b> a skos:Concept ;
    skos:broader <c6f155d3-0877-4b8a-812b-1253f0516c21> ;
    skos:inScheme <> ;
    skos:notation "5.1.1.4" ;
    skos:prefLabel "nutzen ausgewählte digitale Technologien zielgerichtet und reflektiert als Werkzeuge der Kommunikation und der Informationsgewinnung."@de .

<052afa66-0eef-49b7-8680-89604f6eb94a> a skos:Concept ;
    skos:broader <2e59731c-bb73-4bdb-b32b-795d6f71afe6> ;
    skos:inScheme <> ;
    skos:notation "2.5.5" ;
    skos:prefLabel "kennen und nutzen die Möglichkeiten digitaler Schreibwerkzeuge zur Formulierung und (formalen) Gestaltung von Texten, z. B. Schrift‐ und Textformatierung, Arrangieren und Re‐Arrangieren von Textbausteinen und Medienobjekten,"@de .

<06a6730c-ce20-40bb-8fd1-5dc2c5c3febc> a skos:Concept ;
    skos:broader <b0e6363b-14e6-40a7-9d71-28705c9fb358> ;
    skos:inScheme <> ;
    skos:notation "2.1.3" ;
    skos:prefLabel "gestalten Texte (handschriftlich und mithilfe digitaler Schreibwerkzeuge) übersichtlich und zielorientiert, bauen sie sinnvoll auf und strukturieren diese: z. B. Blattaufteilung, Seitenränder, Absatzstrukturen, Textformatierung und Layout."@de .

<0707cc96-ed58-40ec-8588-d9816dc9cd86> a skos:Concept ;
    skos:broader <b0e6363b-14e6-40a7-9d71-28705c9fb358> ;
    skos:inScheme <> ;
    skos:notation "2.1.2" ;
    skos:prefLabel "schreiben Texte (handschriftlich leserlich und mithilfe digitaler Schreibwerkzeuge) in einem der Situation angemessenen Tempo, z. B. persönliche Notiz vs. handschriftlicher Brief,"@de .

<089fba9c-e5b3-48b8-9958-168f49bd9fb0> a skos:Concept ;
    skos:broader <2189034d-a966-4d3d-a380-67baf1aaef16> ;
    skos:inScheme <> ;
    skos:notation "1.3.4" ;
    skos:prefLabel "nehmen in Gesprächen verschiedene Rollen (z. B. Moderation) ein und richten diese an den jeweiligen Zielen aus,"@de .

<0912face-644f-43b6-80e7-81c49f1a963c> a skos:Concept ;
    skos:broader <6f2857a7-3605-4961-af62-18f307ec6232> ;
    skos:inScheme <> ;
    skos:notation "2.6.6" ;
    skos:prefLabel "kennen sowohl allgemeine als auch textsortenspezifische Überarbeitungsstrategien und wenden diese (auch kooperativ) an. Sie reflektieren sowohl ihren Überarbeitungsprozess als auch den Strategieeinsatz."@de .

<10ffbaac-5906-4539-a067-205df03c4b70> a skos:Concept ;
    skos:broader <2e59731c-bb73-4bdb-b32b-795d6f71afe6> ;
    skos:inScheme <> ;
    skos:notation "2.5.9" ;
    skos:prefLabel "integrieren Textbelege und andere Quellen formal richtig in den eigenen Text (zitieren, paraphrasieren),"@de .

<1100e8e8-46c8-4055-ab0a-814d041c37a5> a skos:Concept ;
    skos:broader <e36f9685-8aa4-42ae-b241-8cc269060855> ;
    skos:inScheme <> ;
    skos:notation "1.2.2.1" ;
    skos:prefLabel "geben Informationen wieder und ziehen im Ansatz auch komplexe Schlussfolgerungen,"@de .

<117ceae3-7139-4093-b268-1c6a70eff1cf> a skos:Concept ;
    skos:broader <22361857-d3b6-4f86-8c94-91c332b9bd23> ;
    skos:inScheme <> ;
    skos:notation "4.4.9" ;
    skos:prefLabel "tauschen sich mit anderen zu Text- bzw. Medienwahrnehmungen, -deutungen und -wertungen aus, setzen sich mit vorgebrachten Deutungen und Argumenten auseinander und formulieren fundierte eigene Auffassungen (u. a. literarisches Gespräch, Auswertung szenischer Gestaltung),"@de .

<11cf59d3-e8b7-4e82-8ff2-19bc64072d82> a skos:Concept ;
    skos:broader <4a5012a8-198b-4b46-84fd-d902f5340df1> ;
    skos:inScheme <> ;
    skos:notation "1.4.4" ;
    skos:prefLabel "planen und gestalten Formen des szenischen Sprechens und Spielens, z. B. szenische Lesung, Szene, Aufführung, Hörspiel, Videoclip, Film, Standbilder, Improvisation."@de .

<12608f01-0cb6-4776-83a3-1cbac001f2dd> a skos:Concept ;
    skos:broader <76f1f32a-caf6-4f23-ad2f-3f9955b95d09> ;
    skos:inScheme <> ;
    skos:notation "5.2.1.2" ;
    skos:prefLabel "nutzen Wörter, Wendungen und Formulierungsmuster funktional und erweitern ihren Wortschatz,"@de .

<14e0df2f-d9b9-47f9-aa6b-3b884e226c3b> a skos:Concept ;
    skos:broader <5415a184-16a1-413a-853c-2038776a4933> ;
    skos:inScheme <> ;
    skos:notation "2.2.1.3" ;
    skos:prefLabel "prägen sich Merkwörter ein (z. B. Eigennamen, Ausnahmeschreibungen),"@de .

<190d0056-8a62-4d47-ba3c-02fbd4563a5e> a skos:Concept ;
    skos:broader <2102561f-07f8-48a2-ae9b-e3959025dbca> ;
    skos:inScheme <> ;
    skos:notation "3.3.3" ;
    skos:prefLabel "nutzen bei digitalen Texten Navigationsstrukturen (u. a. Hyperlinks) zur Gewinnung von Textinformationen,"@de .

<1996b2b3-bd54-46d5-b11d-0f5128da67d9> a skos:Concept ;
    skos:broader <e36f9685-8aa4-42ae-b241-8cc269060855> ;
    skos:inScheme <> ;
    skos:notation "1.2.2.2" ;
    skos:prefLabel "erfassen dem Lernstand entsprechende (komplexere) Hörtexte, Gesprächsphasen oder Gespräche (z. B. zentrale Aussagen sowie Sprech- und Gesprächsabsichten),"@de .

<1af7cbd8-52c4-4424-bc90-6f88eb40446d> a skos:Concept ;
    skos:broader <6f2857a7-3605-4961-af62-18f307ec6232> ;
    skos:inScheme <> ;
    skos:notation "2.6.2" ;
    skos:prefLabel "formulieren und nutzen textsortenspezifische Kriterien für die Textüberprüfung und -überarbeitung,"@de .

<1ba2f57f-18d8-4989-b27a-4674036e5920> a skos:Concept ;
    skos:broader <42cf59fc-7ce7-4687-a440-49e891cf78fa> ;
    skos:inScheme <> ;
    skos:notation "5.1.3.4" ;
    skos:prefLabel "vergleichen sprachliche Phänomene in verschiedenen Sprachen (u. a. Herkunftssprachen, Fremdsprachen), auch im Hinblick auf individuelle und gesellschaftliche Mehrsprachigkeit."@de .

<1ba74796-bf20-4684-a4d9-eafec9a5f24b> a skos:Concept ;
    skos:broader <19dc12cd-13bf-4207-af31-acaf5caa9eec> ;
    skos:inScheme <> ;
    skos:notation "3.4.1" ;
    skos:prefLabel "bereiten das Lesen vor (u. a. Leseziel formulieren, Vorwissen aktivieren, Erwartungen an den Text formulieren, nach Möglichkeit die Textsorte klären),"@de .

<1bb5a047-3077-4742-8d67-0f76275aa3f4> a skos:Concept ;
    skos:broader <2e59731c-bb73-4bdb-b32b-795d6f71afe6> ;
    skos:inScheme <> ;
    skos:notation "2.5.1" ;
    skos:prefLabel "formulieren Texte gedanklich geordnet, verständlich, kohärenzstiftend und strukturiert, sprachlich angemessen, abwechslungsreich und differenziert,"@de .

<1ed88f00-58bb-4a45-baf4-0b61fdd18c08> a skos:Concept ;
    skos:broader <2189034d-a966-4d3d-a380-67baf1aaef16> ;
    skos:inScheme <> ;
    skos:notation "1.3.7" ;
    skos:prefLabel "sprechen über das sprachliche und literarische Lernen sowie über Lernerfahrungen: geben Beobachtungen wieder, beschreiben Sachverhalte, geben Erklärungen und Begründungen, reflektieren."@de .

<21fd84c1-2ab8-4dcd-bdb7-b694a291ddb5> a skos:Concept ;
    skos:broader <b8bc877f-1ecb-4c94-94ed-a0a2a625b6a6> ;
    skos:inScheme <> ;
    skos:notation "2.4.1" ;
    skos:prefLabel "sammeln Ideen, ordnen Informationen und fertigen eine Gliederung an, z. B. numerische Gliederung, Cluster, Mindmap, Flussdiagramm,"@de .

<2274cae1-d185-4b1c-9625-c2569b4fe74b> a skos:Concept ;
    skos:broader <9da46cf9-9f12-4151-9d66-9c8878b8062b> ;
    skos:inScheme <> ;
    skos:notation "5.1.2.3" ;
    skos:prefLabel "unterscheiden grundlegende Textfunktionen – Darstellen (z. B. Informieren in einer Zeitungsmeldung), Appellieren (z. B. Überzeugen in einer Werbeanzeige; Gesetzestext), Ausdrücken (z. B. sich selbst ausdrücken in einem Bewerbungsschreiben, einer Beschwerde oder einem Gedicht) – und nutzen ihr Wissen über Textfunktionen für das eigene Schreiben und für die Erschließung von Texten."@de .

<23075b74-314e-4a0f-85ef-b6615ab48de1> a skos:Concept ;
    skos:broader <4fd39d1f-9d48-4b30-80fc-619eb93dc14f> ;
    skos:inScheme <> ;
    skos:notation "2.2.6" ;
    skos:prefLabel "verfügen über Fehleraufmerksamkeit und nutzen Strategien zur Klärung orthografischer Fragen,"@de .

<26252485-0275-4588-b89c-850b0d1f5afa> a skos:Concept ;
    skos:broader <42cf59fc-7ce7-4687-a440-49e891cf78fa> ;
    skos:inScheme <> ;
    skos:notation "5.1.3.3" ;
    skos:prefLabel "untersuchen ausgewählte Erscheinungen des Sprachwandels (z. B. Bedeutungswandel, Entlehnungen aus anderen Sprachen, Sprachwandel durch digitale Kommunikation),"@de .

<26c94172-3245-470f-a549-696f4f8f2515> a skos:Concept ;
    skos:broader <7397461d-c0d2-4350-aa6f-75bdb9c12752> ;
    skos:inScheme <> ;
    skos:notation "1.1.8" ;
    skos:prefLabel "geben in vertrauten und unvertrauten Situationen gezielt Feedback zu Redebeiträgen,"@de .

<2a80353d-5090-4df3-8168-9b91dfdd2dc8> a skos:Concept ;
    skos:broader <bc225a0a-cfca-43e1-9730-cd5d9cb72101> ;
    skos:inScheme <> ;
    skos:notation "4.1.4" ;
    skos:prefLabel "stellen an ausgewählten Beispielen Zusammenhänge zwischen Text und Kontext her (insbes. Entstehungszeit, Epoche, Leben des Autors/der Autorin),"@de .

<2b612bb6-bea4-4741-8c46-1d84993e8e0e> a skos:Concept ;
    skos:broader <39954c59-dd5b-4e07-a277-8ca3923fb151> ;
    skos:inScheme <> ;
    skos:notation "1.1.3.2" ;
    skos:prefLabel "informieren: berichten, beschreiben,"@de .

<2bbf1861-b1a1-4126-a532-a80f6ed7866b> a skos:Concept ;
    skos:broader <039ab014-4391-4436-95d1-263320901f1f> ;
    skos:inScheme <> ;
    skos:notation "2.5.3.2" ;
    skos:prefLabel "Darstellen: Informieren (Beschreiben, Berichten), Erklären, Instruieren, (epistemisch) gedanklich Klären, Erörtern, z. B. in den Textsorten: Orts‐, Funktions‐ oder Bildbeschreibung, Erlebnisbericht, Lebenslauf, Protokoll, Nachricht, (Online)Lexikon‐Eintrag, Artikel für Schülerzeitung, Gebrauchsanweisung, Konstruktions‐ oder Spielanleitung, Zusammenfassung,"@de .

<2e0fd5b8-742b-4b19-a7a2-c8c1d6669b1d> a skos:Concept ;
    skos:broader <6f2857a7-3605-4961-af62-18f307ec6232> ;
    skos:inScheme <> ;
    skos:notation "2.6.1" ;
    skos:prefLabel "erkennen den Überarbeitungsbedarf von Texten, überlegen Schritte zur Textüberarbeitung und setzen diese um,"@de .

<2e181d35-3c4a-4782-98b9-37d2cb3d23e3> a skos:Concept ;
    skos:broader <c6f155d3-0877-4b8a-812b-1253f0516c21> ;
    skos:inScheme <> ;
    skos:notation "5.1.1.1" ;
    skos:prefLabel "unterscheiden beim Sprachhandeln die Handlungs-, Inhalts- und Beziehungsebene und untersuchen deren Zusammenwirken in Bezug auf gelingende vs. misslingende Kommunikation, öffentliche vs. private Kommunikationssituationen,"@de .

<2e46a7f2-f2e9-40cf-9298-fbf20b597989> a skos:Concept ;
    skos:broader <22361857-d3b6-4f86-8c94-91c332b9bd23> ;
    skos:inScheme <> ;
    skos:notation "4.4.2" ;
    skos:prefLabel "nutzen Fachbegriffe bei der Erschließung (u. a. Autor, Erzähler, Erzählperspektive, Erzählzeit/erzählte Zeit, Figur, Monolog, Dialog, Szene, Regieanweisung; Reim, Vers, Sprecher/lyrischer Sprecher; Kameraperspektive, Einstellungsgröße, Montage, Kamerabewegung, Schnitt),"@de .

<30f5b5b6-1095-4dc3-8f63-72fdad63e72d> a skos:Concept ;
    skos:broader <721bc999-7ee3-4b37-a1e7-eed4947a4703> ;
    skos:inScheme <> ;
    skos:notation "3.1.4" ;
    skos:prefLabel "lesen in angemessenem Tempo."@de .

<3500bd74-28c6-4c1d-8d52-d3838e8d0997> a skos:Concept ;
    skos:broader <76f1f32a-caf6-4f23-ad2f-3f9955b95d09> ;
    skos:inScheme <> ;
    skos:notation "5.2.1.5" ;
    skos:prefLabel "ermitteln den funktionalen Beitrag von Wörtern verschiedener Wortarten zum Aufbau von Sätzen,"@de .

<362a2186-ee81-468c-b36d-544b0e13851a> a skos:Concept ;
    skos:broader <6f2857a7-3605-4961-af62-18f307ec6232> ;
    skos:inScheme <> ;
    skos:notation "2.6.4" ;
    skos:prefLabel "holen Text-Feedback ein und formulieren selbst auch Feedback zu Texten anderer, "@de .

<368aec91-aecb-4ec1-84fd-0e197d89629d> a skos:Concept ;
    skos:broader <22361857-d3b6-4f86-8c94-91c332b9bd23> ;
    skos:inScheme <> ;
    skos:notation "4.4.10" ;
    skos:prefLabel "nutzen handlungs- und produktionsorientierte Verfahren, um ihre Deutung zu entwickeln und darzustellen und um Strukturen literarischer Texte nachzuvollziehen (z. B. innerer Monolog, Standbild, Parallelgedicht; Clip, Storyboard/ Fotostory),"@de .

<373b00ce-6880-4804-b8f6-0f17b14dda82> a skos:Concept ;
    skos:broader <eacb0d75-189a-4c26-8b10-743e4fc2bdff> ;
    skos:inScheme <> ;
    skos:notation "2.6.3.2" ;
    skos:prefLabel "Angemessenheit sprachlicher Gestaltungsmittel, Rechtschreibung und Grammatik,"@de .

<38722203-4c0e-417c-a9fc-fee3c1f3dcdb> a skos:Concept ;
    skos:broader <4fd39d1f-9d48-4b30-80fc-619eb93dc14f> ;
    skos:inScheme <> ;
    skos:notation "2.2.5" ;
    skos:prefLabel "kennen und nutzen die Interpunktionszeichen: Punkt, Komma, Fragezeichen, Ausrufezeichen, Anführungszeichen, Semikolon, Doppelpunkt, Auslassungspunkte, Gedankenstrich, Klammern,"@de .

<3f1dcd9b-7d22-494a-ba7f-d0a898d84032> a skos:Concept ;
    skos:broader <721bc999-7ee3-4b37-a1e7-eed4947a4703> ;
    skos:inScheme <> ;
    skos:notation "3.1.3" ;
    skos:prefLabel "lesen komplexere Texte sinngestaltend vor,"@de .

<3fb03ac1-9139-4126-a75f-13102f0c2768> a skos:Concept ;
    skos:broader <3f2dacc3-59f8-46a6-be81-a5b862906f52> ;
    skos:inScheme <> ;
    skos:notation "4.5.4" ;
    skos:prefLabel "unterscheiden informierende, erzählende und appellierende Elemente in Texten,"@de .

<42d30c3d-afa2-4da0-9f59-d055bc1707f2> a skos:Concept ;
    skos:broader <bc225a0a-cfca-43e1-9730-cd5d9cb72101> ;
    skos:inScheme <> ;
    skos:notation "4.1.1" ;
    skos:prefLabel "kennen ein Spektrum von Werken bedeutender Autorinnen und Autoren und setzen sich mit den Texten und ihren Leseerfahrungen auseinander,"@de .

<45924d3a-9a9b-47e8-ba8c-6bcf50abd4cd> a skos:Concept ;
    skos:broader <4fd39d1f-9d48-4b30-80fc-619eb93dc14f> ;
    skos:inScheme <> ;
    skos:notation "2.2.8" ;
    skos:prefLabel "überprüfen und korrigieren Texte auf orthografische Richtigkeit."@de .

<462e5874-ef2a-408f-8acc-d7c4aa39f83a> a skos:Concept ;
    skos:broader <039ab014-4391-4436-95d1-263320901f1f> ;
    skos:inScheme <> ;
    skos:notation "2.5.3.3" ;
    skos:prefLabel "Darstellen: Informieren auf Basis klar strukturierter Material- und Textquellen (informierend materialgestützt schreiben) "@de .

<46b43497-940b-489a-929e-bee6a32312f3> a skos:Concept ;
    skos:broader <b8bc877f-1ecb-4c94-94ed-a0a2a625b6a6> ;
    skos:inScheme <> ;
    skos:notation "2.4.7" ;
    skos:prefLabel "planen Texte kooperativ und kollaborativ und strukturieren diese: z. B. Textbaupläne,"@de .

<46c4c70b-4ac7-43d9-ab56-d7097a900bb4> a skos:Concept ;
    skos:broader <39954c59-dd5b-4e07-a277-8ca3923fb151> ;
    skos:inScheme <> ;
    skos:notation "1.1.3.4" ;
    skos:prefLabel "erläutern,"@de .

<4a465826-712b-4bb6-b0ed-42a4c1180acb> a skos:Concept ;
    skos:broader <22361857-d3b6-4f86-8c94-91c332b9bd23> ;
    skos:inScheme <> ;
    skos:notation "4.4.7" ;
    skos:prefLabel "überprüfen eigene und fremde Deutungshypothesen,"@de .

<4a55ece8-b2cf-4b32-ab30-43d6386147d0> a skos:Concept ;
    skos:broader <1d3a9525-90f8-46f9-95af-aabce9e55990> ;
    skos:inScheme <> ;
    skos:notation "5.2.2.1" ;
    skos:prefLabel "untersuchen vergleichend den Aufbau von Texten und/oder Gesprächen strukturell und funktional unter Anwendung von Fachbegriffen (s. Übersicht: Grundlegende sprachliche Struktureinheiten) und ermitteln Gemeinsamkeiten und Unterschiede,"@de .

<4fa2f545-373e-416b-9cba-2a6164adb253> a skos:Concept ;
    skos:broader <39954c59-dd5b-4e07-a277-8ca3923fb151> ;
    skos:inScheme <> ;
    skos:notation "1.1.3.3" ;
    skos:prefLabel "erklären,"@de .

<5204c1a3-84cb-4a39-8f41-102f46c43bac> a skos:Concept ;
    skos:broader <2189034d-a966-4d3d-a380-67baf1aaef16> ;
    skos:inScheme <> ;
    skos:notation "1.3.1" ;
    skos:prefLabel "gestalten vertraute und unvertraute Gesprächssituationen verständlich, zuhörerbezogen, themenbezogen und konstruktiv,"@de .

<526d9201-d865-42b5-8133-dff1d795c534> a skos:Concept ;
    skos:broader <721bc999-7ee3-4b37-a1e7-eed4947a4703> ;
    skos:inScheme <> ;
    skos:notation "3.1.1" ;
    skos:prefLabel "nutzen die schriftsprachlichen Informationen (u. a. Silbe, Morphem, Komma, Punkt, Fragezeichen, Großschreibung) beim Lesen,"@de .

<52725f98-0419-4732-a215-786e54cdb772> a skos:Concept ;
    skos:broader <039ab014-4391-4436-95d1-263320901f1f> ;
    skos:inScheme <> ;
    skos:notation "2.5.3.5" ;
    skos:prefLabel "Appellieren: Auffordern, Bitten, Wünschen, Begründen (Argumentieren), eine Textdeutung belegen, Erörtern, Überzeugen, Kommentieren, z. B. in den Textsorten: Aufruf, Werbeanzeige, Stellungnahme, Antrag, Bewerbungsanschreiben, Erörterung, Kommentar, Leserbrief, Interpretation, Buch‐, Spiel‐ oder Filmkritik (Rezension),"@de .

<5dda1206-b5b0-42f7-b833-62c4f7958f77> a skos:Concept ;
    skos:broader <b8bc877f-1ecb-4c94-94ed-a0a2a625b6a6> ;
    skos:inScheme <> ;
    skos:notation "2.4.3" ;
    skos:prefLabel "stellen Überlegungen zu den Adressaten (unbekannt, verschiedene, abstrakt), deren unterschiedlichem Vorwissen und ihren Lesezielen an,"@de .

<5fd47ad8-b3ee-47c6-8d6e-8ce26400f6fb> a skos:Concept ;
    skos:broader <76f1f32a-caf6-4f23-ad2f-3f9955b95d09> ;
    skos:inScheme <> ;
    skos:notation "5.2.1.6" ;
    skos:prefLabel "unterscheiden zentrale grammatische Mittel hinsichtlich ihrer Struktur und ihrer Funktion im sprachlichen Handeln (z. B. Tempus, Modus, Genus verbi; Genus, Numerus, Kasus; Komparation)."@de .

<6106d77e-0d23-44d3-8cbc-18f7fbf6620f> a skos:Concept ;
    skos:broader <3f2dacc3-59f8-46a6-be81-a5b862906f52> ;
    skos:inScheme <> ;
    skos:notation "4.5.5" ;
    skos:prefLabel "stellen Zusammenhänge zwischen Intentionen, Textmerkmalen, sprachlichen Gestaltungsmitteln und Wirkungen her,"@de .

<6165dfe1-8de4-41e8-b34c-e12216f4705b> a skos:Concept ;
    skos:broader <7397461d-c0d2-4350-aa6f-75bdb9c12752> ;
    skos:inScheme <> ;
    skos:notation "1.1.1" ;
    skos:prefLabel "äußern sich verständlich und situationsadäquat auch in der gesprochenen Standardsprache,"@de .

<61a15c18-71e9-41ec-9b09-5d7f142dfed3> a skos:Concept ;
    skos:broader <4fd39d1f-9d48-4b30-80fc-619eb93dc14f> ;
    skos:inScheme <> ;
    skos:notation "2.2.3" ;
    skos:prefLabel "schreiben auch wichtige Fachbegriffe und Fremdwörter richtig,"@de .

<62723c18-f4f1-4d8d-a483-1644a5593c3a> a skos:Concept ;
    skos:broader <22361857-d3b6-4f86-8c94-91c332b9bd23> ;
    skos:inScheme <> ;
    skos:notation "4.4.5" ;
    skos:prefLabel "stellen Bezüge zwischen literarischen Texten, Medien und deren Motiven her und nutzen Vergleiche für die Deutung und Wertung (auch intermedial, z. B. Buch und Film),"@de .

<63136bd2-0135-4caa-8a7b-eea4a76b844d> a skos:Concept ;
    skos:broader <5415a184-16a1-413a-853c-2038776a4933> ;
    skos:inScheme <> ;
    skos:notation "2.2.1.4" ;
    skos:prefLabel "unterscheiden zwischen Kernwörtern und Fremdwörtern,"@de .

<65183241-d967-4574-a281-30af63340d37> a skos:Concept ;
    skos:broader <4fd39d1f-9d48-4b30-80fc-619eb93dc14f> ;
    skos:inScheme <> ;
    skos:notation "2.2.2" ;
    skos:prefLabel "schreiben Kernwörter richtig (Inhaltswörter und Funktionswörter)"@de .

<67bff618-b177-487f-a8d6-17ce0bfb7024> a skos:Concept ;
    skos:broader <2189034d-a966-4d3d-a380-67baf1aaef16> ;
    skos:inScheme <> ;
    skos:notation "1.3.6" ;
    skos:prefLabel "wenden ein Spektrum von allgemeinen und für Gesprächs- und Lernformen spezifischen Strategien an, z. B. Gespräche steuern (Diskussionen), zielführende Rückmeldungen geben (Feedback),"@de .

<69a1a8c9-4901-4f10-a3de-ba5d734434b9> a skos:Concept ;
    skos:broader <bc225a0a-cfca-43e1-9730-cd5d9cb72101> ;
    skos:inScheme <> ;
    skos:notation "4.1.8" ;
    skos:prefLabel "nutzen Angaben zum Wirklichkeitsbezug von Texten und Medien als Rezeptionshinweise (z. B. Gattungs-/Genrebezeichnungen: Roman, Satire, Krimi, Biopic, Dokumentation, Doku-Drama) und setzen sich mit Beziehungen zwischen Wirklichkeit und Fiktion auseinander,"@de .

<69bcc697-ee38-4277-89e4-ac8656ccbd57> a skos:Concept ;
    skos:broader <c6f155d3-0877-4b8a-812b-1253f0516c21> ;
    skos:inScheme <> ;
    skos:notation "5.1.1.3" ;
    skos:prefLabel "untersuchen Normen demokratischer Gesprächs- und Kommunikationskultur und sprachlicher Höflichkeit (auch in digitalen Umgebungen) und nutzen diese versiert für die sprachliche Gestaltung eigener Äußerungen sowie für den Umgang mit den sprachlichen Äußerungen anderer,"@de .

<6a058190-1f0e-4f11-b0b0-1aebb6296f9d> a skos:Concept ;
    skos:broader <4a5012a8-198b-4b46-84fd-d902f5340df1> ;
    skos:inScheme <> ;
    skos:notation "1.4.3" ;
    skos:prefLabel "planen, organisieren und gestalten Formen des Präsentierens, z. B. Standbild, Stegreif-Rede, Referat, Podcasts, Erklärvideos,"@de .

<6a1743d2-968e-4a18-b1b3-3a4509757bfb> a skos:Concept ;
    skos:broader <e36f9685-8aa4-42ae-b241-8cc269060855> ;
    skos:inScheme <> ;
    skos:notation "1.2.2.3" ;
    skos:prefLabel "beurteilen auch komplexe Aussagen zum Gehörten begründet,"@de .

<6a21a891-dffb-40e6-8b56-2d68e7519e97> a skos:Concept ;
    skos:broader <b8bc877f-1ecb-4c94-94ed-a0a2a625b6a6> ;
    skos:inScheme <> ;
    skos:notation "2.4.4" ;
    skos:prefLabel "klären entsprechend dem Schreibziel die Schreibabsicht, Schreibsituation und Textfunktion,"@de .

<6c63721c-3609-4773-89df-b778289a4663> a skos:Concept ;
    skos:broader <b8bc877f-1ecb-4c94-94ed-a0a2a625b6a6> ;
    skos:inScheme <> ;
    skos:notation "2.4.8" ;
    skos:prefLabel "nutzen und reflektieren die Möglichkeiten digitaler Schreibwerkzeuge zur Vorbereitung der Textproduktion, z. B. Anfertigen von Notizen und Skizzen, Anlegen von Stoffsammlungen, Exzerpieren aus digitalen Informationsressourcen,"@de .

<7487f88a-0264-4788-aaf0-a644666717e5> a skos:Concept ;
    skos:broader <22361857-d3b6-4f86-8c94-91c332b9bd23> ;
    skos:inScheme <> ;
    skos:notation "4.4.1" ;
    skos:prefLabel "nutzen implizite und explizite Informationen zu Figuren und Figurenkonstellationen, zu Raum- und Zeitdarstellung, Handlungs- und Konfliktverlauf sowie Atmosphäre zum Aufbau von Textverständnis und zur Entwicklung einer differenzierten Deutung,"@de .

<7488f1d7-bfb9-4572-aa62-f077dc48438c> a skos:Concept ;
    skos:broader <2e59731c-bb73-4bdb-b32b-795d6f71afe6> ;
    skos:inScheme <> ;
    skos:notation "2.5.6" ;
    skos:prefLabel "erstellen Schaubilder, Diagramme und Tabellen und integrieren diese sprachlich in den eigenen – auch digitalen – Text,"@de .

<7ad496ba-f9fa-4ee4-bd71-8f90d2fefff8> a skos:Concept ;
    skos:broader <22361857-d3b6-4f86-8c94-91c332b9bd23> ;
    skos:inScheme <> ;
    skos:notation "4.4.6" ;
    skos:prefLabel "vergleichen Handlungen und Handlungsmotive von Figuren mit vertrauten Handlungen und Handlungsmotiven und nehmen begründete Wertungen vor,"@de .

<7b8c6d52-b9f8-420a-9545-8b61843f8d05> a skos:Concept ;
    skos:broader <039ab014-4391-4436-95d1-263320901f1f> ;
    skos:inScheme <> ;
    skos:notation "2.5.3.1" ;
    skos:prefLabel "Ausdrücken: Sich selbst, Erlebtes und Erdachtes, Gedanken und Gefühle, u. a. in den Textsorten: Selbstportrait, (Lern‐)Tagebuch, Bewerbungsanschreiben, Gedicht,"@de .

<7c63101b-b51d-4f7d-bc59-cf14efe81add> a skos:Concept ;
    skos:broader <bc225a0a-cfca-43e1-9730-cd5d9cb72101> ;
    skos:inScheme <> ;
    skos:notation "4.1.6" ;
    skos:prefLabel "unterscheiden zentrale Textmuster (Erzählen, Berichten, Beschreiben, Argumentieren),"@de .

<7d0e94aa-98ef-4626-ba98-1c324d8e1863> a skos:Concept ;
    skos:broader <c6f155d3-0877-4b8a-812b-1253f0516c21> ;
    skos:inScheme <> ;
    skos:notation "5.1.1.2" ;
    skos:prefLabel "untersuchen Merkmale der Adressatenorientierung und beschreiben ihre Bedeutung für das angemessene sprachliche Handeln,"@de .

<7d140cb2-9087-403b-8326-4d1f9d5a7436> a skos:Concept ;
    skos:broader <375a0347-d54e-4073-aa5b-5def3b5a1ed7> ;
    skos:inScheme <> ;
    skos:notation "5.2.4.1" ;
    skos:prefLabel "nutzen die Konventionen der gesprochenen Standardsprache weitgehend routiniert und situationsangemessen."@de .

<7d18aef3-f2e5-419e-b513-d9ced8f70304> a skos:Concept ;
    skos:broader <3f33fc76-3b95-440c-b452-c550392bc086> ;
    skos:definition "Die Schülerinnen und Schüler beachten bei der Planung und Strukturierung, beim Formulieren und beim Überarbeiten von Texten allgemeine Textmerkmale, Adressaten, den Schreibanlass, Schreibfunktion und Textsorte, Formen des Schreibens und den Einsatz digitaler Schreibwerkzeuge, den Umgang mit Quellen beim Belegen und Zitieren und die Anwendung von Schreibstrategien."@de ;
    skos:inScheme <> ;
    skos:notation "2.3" ;
    skos:prefLabel "Texte verfassen"@de .

<7e68a0cf-cb05-42e0-afeb-5c9d1f1164ff> a skos:Concept ;
    skos:broader <7397461d-c0d2-4350-aa6f-75bdb9c12752> ;
    skos:inScheme <> ;
    skos:notation "1.1.4" ;
    skos:prefLabel "nutzen ihre Vorbereitungen (z. B. Notizen) für die sach- und situationsgerechte, differenzierte Gestaltung vertrauter und unvertrauter Situationen, z. B. Bewerbungsgespräch, Diskussion, Debatte, Präsentation,"@de .

<81bf0fa7-653a-4383-9b56-dc0ad395f10b> a skos:Concept ;
    skos:broader <4a5012a8-198b-4b46-84fd-d902f5340df1> ;
    skos:inScheme <> ;
    skos:notation "1.4.1" ;
    skos:prefLabel "leisten längere, auch freie Beiträge adressatenorientiert und mit Hilfe eines Stichwortzettels/einer Gliederung/von Notizen/eines Textes, z. B. Referat, Präsentation, Rede, Gedicht,"@de .

<81c016ea-dda1-4769-a1d6-2a8b7afe2d87> a skos:Concept ;
    skos:broader <bc225a0a-cfca-43e1-9730-cd5d9cb72101> ;
    skos:inScheme <> ;
    skos:notation "4.1.7" ;
    skos:prefLabel "unterscheiden pragmatische Textsorten und deren zentrale Funktionen (u. a. Bericht, Nachricht, Feature, Reportage [informierend]; Anleitung, Erklärvideo [instruierend]; Schulordnung [regulierend]; Kommentar, Buchkritik [überzeugend, argumentierend]),"@de .

<842507a4-0dd2-44f8-9394-985bc52b5537> a skos:Concept ;
    skos:broader <039ab014-4391-4436-95d1-263320901f1f> ;
    skos:inScheme <> ;
    skos:notation "2.5.3.4" ;
    skos:prefLabel "Darstellen von Textbearbeitungen: Inhaltsangabe, Ergebnisdarstellung einer Textuntersuchung entsprechend den Anforderungen des Erschließens von Texten und Medien"@de .

<85b7b641-541c-4461-8da8-68f71f3310e5> a skos:Concept ;
    skos:broader <2e59731c-bb73-4bdb-b32b-795d6f71afe6> ;
    skos:inScheme <> ;
    skos:notation "2.5.8" ;
    skos:prefLabel "formulieren eigene Positionen und begründen diese"@de .

<87868e15-3220-440d-abaf-b8ad542dc16f> a skos:Concept ;
    skos:broader <b96723ae-b913-4225-bdd3-f15173caae34> ;
    skos:inScheme <> ;
    skos:notation "1.2.1.2" ;
    skos:prefLabel "Verstehen und Nicht-Verstehen zum Ausdruck bringen, z. B. durch Nachfragen, durch Kommentare,"@de .

<8a51fe0f-9c91-4979-a1e7-2ea32737f9b8> a skos:Concept ;
    skos:broader <6f2857a7-3605-4961-af62-18f307ec6232> ;
    skos:inScheme <> ;
    skos:notation "2.6.5" ;
    skos:prefLabel "nutzen bei der Textüberarbeitung digitale Schreibwerkzeuge (auch kooperativ), z. B. Umstellen und Modifizieren von Textbausteinen und Medienobjekten, Nachverfolgen von Änderungen, Kommentarfunktion, digitale Korrekturhilfen, und reflektieren diese,"@de .

<8bd0f3c0-b6a9-435b-8ef6-1b91d473d59b> a skos:Concept ;
    skos:broader <2e59731c-bb73-4bdb-b32b-795d6f71afe6> ;
    skos:inScheme <> ;
    skos:notation "2.5.4" ;
    skos:prefLabel "nutzen unterschiedliche Schreibformen: kooperative (z. B. Schreibkonferenzen), produktive (z. B. Umschreiben, Weiterschreiben) und kreative (z. B. Schreiben zu Bildern),"@de .

<8e5514b3-83a3-40bc-b89c-d6018fbd5451> a skos:Concept ;
    skos:broader <dc83bfcf-b0ef-4cf2-a4f1-58716b83c2c4> ;
    skos:inScheme <> ;
    skos:notation "1.2.3" ;
    skos:prefLabel "wählen aus einem Repertoire kognitiver und metakognitive Strategien aus, die sie vor, während und nach dem Zuhören einsetzen, z. B. die Erwartungshaltung zielorientiert steuern, Aufmerksamkeit steuern, Informationen sichern und zusammenfassen: Notizen, Protokoll."@de .

<8ef59e73-fbb0-450b-b753-4301f17d3770> a skos:Concept ;
    skos:broader <22361857-d3b6-4f86-8c94-91c332b9bd23> ;
    skos:inScheme <> ;
    skos:notation "4.4.4" ;
    skos:prefLabel "formulieren Wertungen von Texten und begründen sie differenziert,"@de .

<91414c38-85c9-4c2b-8dd1-db376202e6df> a skos:Concept ;
    skos:broader <3f2dacc3-59f8-46a6-be81-a5b862906f52> ;
    skos:inScheme <> ;
    skos:notation "4.5.3" ;
    skos:prefLabel "unterscheiden Information und Wertung in Texten,"@de .

<917bb4ee-ea4b-49c0-adff-f6c55cfeb161> a skos:Concept ;
    skos:broader <22361857-d3b6-4f86-8c94-91c332b9bd23> ;
    skos:inScheme <> ;
    skos:notation "4.4.3" ;
    skos:prefLabel "formulieren eigene Deutungen von Texten und belegen diese,"@de .

<95b49f72-5fbb-44e5-b733-59c5fc151cf1> a skos:Concept ;
    skos:broader <39954c59-dd5b-4e07-a277-8ca3923fb151> ;
    skos:inScheme <> ;
    skos:notation "1.1.3.1" ;
    skos:prefLabel "erzählen,"@de .

<97d41ac7-b3d5-4b99-a481-7f77a3964121> a skos:Concept ;
    skos:broader <2e59731c-bb73-4bdb-b32b-795d6f71afe6> ;
    skos:inScheme <> ;
    skos:notation "2.5.7" ;
    skos:prefLabel "nehmen Gedanken und deutlich markierte Positionen anderer in den eigenen Text auf und positionieren sich zu ihnen,"@de .

<97faeb68-19c9-4931-bcc8-c0dcfb8f9804> a skos:Concept ;
    skos:broader <2e59731c-bb73-4bdb-b32b-795d6f71afe6> ;
    skos:inScheme <> ;
    skos:notation "2.5.2" ;
    skos:prefLabel "formulieren Texte auch für unbekannte, unterschiedliche und abstrakte Adressaten angemessen, "@de .

<98fa5b41-0c88-4ebc-8d60-b1b299840182> a skos:Concept ;
    skos:broader <3f2dacc3-59f8-46a6-be81-a5b862906f52> ;
    skos:inScheme <> ;
    skos:notation "4.5.2" ;
    skos:prefLabel "werten auch komplexere nichtlineare und multimodale Texte (Text-Bild-Bezüge) zielorientiert aus, z. B. um über ein Thema zu sprechen oder zu schreiben,"@de .

<9c765c10-0405-484d-97d0-8ea101be8a0d> a skos:Concept ;
    skos:broader <c8e55883-cca0-4666-a017-a7f8f456a7ed> ;
    skos:inScheme <> ;
    skos:notation "4.6.1" ;
    skos:prefLabel "kennen medienspezifische Gestaltungsmittel und beschreiben ihre Wirkung,"@de .

<9cbf189e-ea0c-4a67-8fac-e1bbefd96ea3> a skos:Concept ;
    skos:broader <7397461d-c0d2-4350-aa6f-75bdb9c12752> ;
    skos:inScheme <> ;
    skos:notation "1.1.6" ;
    skos:prefLabel "berücksichtigen bewusst ein Spektrum von Wirkungen ihrer Redeweise situations- und adressatengerecht: Lautstärke, Betonung, Sprechtempo, Pausen, Körpersprache (Gestik/Mimik/Körperhaltung),"@de .

<9ec520f8-53eb-4ceb-9bed-e67b51c4afd5> a skos:Concept ;
    skos:broader <8bff77c3-5779-49f8-9a9e-b51eb5a8b679> ;
    skos:inScheme <> ;
    skos:notation "4.2.2" ;
    skos:prefLabel "bewerten Rechercheergebnisse (insbes. Internet) im Hinblick auf ihre thematische Relevanz und Verlässlichkeit (Informationsangebote vergleichen, Quellenangaben und -qualität prüfen, Informationen zu Autorinnen und Autoren recherchieren, mögliche Textfunktionen beachten)."@de .

<9fb0fac7-8557-49b6-b255-e2fc826f9e25> a skos:Concept ;
    skos:broader <9da46cf9-9f12-4151-9d66-9c8878b8062b> ;
    skos:inScheme <> ;
    skos:notation "5.1.2.2" ;
    skos:prefLabel "untersuchen die Anforderungen an die Rollen von Sprecher/Schreiber – Hörer/Leser in Hinblick auf gelingende Kommunikation und werden diesen im eigenen sprachlichen Handeln gerecht,"@de .

<9fd71bcf-0b51-4e2d-b6fe-858316b2a2fc> a skos:Concept ;
    skos:broader <1cae99e9-be54-4bb0-a485-785ef6eeb792> ;
    skos:inScheme <> ;
    skos:notation "5.2.5.1" ;
    skos:prefLabel "nutzen Wörterbücher, Grammatiken, digitale Rechtschreibhilfen und Informationsangebote zum Deutschen (z. B. OWID, grammis) für das Nachschlagen von Schreibungen, grammatischen Verwendungsweisen und Verwendungskontexten."@de .

<a0f86f55-88a3-4733-a2bc-68d8fc2afb38> a skos:Concept ;
    skos:broader <42cf59fc-7ce7-4687-a440-49e891cf78fa> ;
    skos:inScheme <> ;
    skos:notation "5.1.3.1" ;
    skos:prefLabel "unterscheiden Ausprägungen von Sprache und Sprachvariation (z. B. Standardsprache – Alltagssprache, Fachsprache, Dialekt, Regionalsprache; Mehrsprachigkeit; Sprache in der digitalen Kommunikation; Deutsche Gebärdensprache) und reflektieren Verwendungsweisen,"@de .

<a20b8511-af30-4882-9ef7-8325ad15d60f> a skos:Concept ;
    skos:broader <2e59731c-bb73-4bdb-b32b-795d6f71afe6> ;
    skos:inScheme <> ;
    skos:notation "2.5.10" ;
    skos:prefLabel "kennen Formulierungsstrategien und wenden diese (auch kooperativ) an. Sie reflektieren ihren Strategieeinsatz."@de .

<a25a6a28-9e48-4f8b-b555-2af709f704fc> a skos:Concept ;
    skos:broader <c8e55883-cca0-4666-a017-a7f8f456a7ed> ;
    skos:inScheme <> ;
    skos:notation "4.6.3" ;
    skos:prefLabel "unterscheiden Nutzungsformen digitaler Formate und Umgebungen und stellen Zusammenhänge zwischen Intentionen, Gestaltung und Wirkung her,"@de .

<a311e8b3-6350-49e0-b479-930df3618091> a skos:Concept ;
    skos:broader <dada0222-b869-4a4a-8971-fa436a9f232c> ;
    skos:inScheme <> ;
    skos:notation "5.2.3.2" ;
    skos:prefLabel "tauschen sich über die Schreibung von Wörtern und Sätzen, auch über orthografische Zweifelsfälle und die Interpunktion aus (z. B. Rechtschreibgespräch, Interpunktionsgespräch)."@de .

<a44b70f4-ace2-4415-8ce6-9a21eca8d5ee> a skos:Concept ;
    skos:broader <76f1f32a-caf6-4f23-ad2f-3f9955b95d09> ;
    skos:inScheme <> ;
    skos:notation "5.2.1.1" ;
    skos:prefLabel "untersuchen Wörter und Wendungen in ihrer Struktur und hinsichtlich ihrer Verwendungsbedingungen, ihrer Bedeutung und ihrer Beziehungen zu anderen Wörtern,"@de .

<a4f0ff23-69be-47ad-a6fd-ee43f5cf9765> a skos:Concept ;
    skos:broader <7397461d-c0d2-4350-aa6f-75bdb9c12752> ;
    skos:inScheme <> ;
    skos:notation "1.1.7" ;
    skos:prefLabel "wählen geeignete (multimodale) Medien sinnvoll aus und setzen diese gezielt ein, um den Inhalt und die Wirkung eigener Sprechbeiträge zu unterstützen,"@de .

<a5700027-e64f-487a-a8f3-e3bc00cb9814> a skos:Concept ;
    skos:broader <2102561f-07f8-48a2-ae9b-e3959025dbca> ;
    skos:inScheme <> ;
    skos:notation "3.3.2" ;
